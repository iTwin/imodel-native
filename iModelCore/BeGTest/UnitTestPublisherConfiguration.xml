<?xml version="1.0" encoding="utf-8"?>

<ImportConfig
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="urn:schemas-bentley-com:xml-dgndbimportconfiguration DgnDbImportConfiguration.xsd"
      >
    <Levels/>
    <Models/>
    <References newLevelDisplay="false" mergeIntoParent="false"/>
    <Raster importAttachments="true" compressData="true"/>
    <ECSchemas exportDiscovered="false" stopPublishingAfterExport="false">
        <SupplementalPaths/>
        <ForcedSelections/>
    </ECSchemas>
    <Thumbnails viewTypes="Physical Drawing" pixelResolution="768" renderModeOverride="None"/>
    <Fonts searchPaths="$(AppRoot)DgnV8/Fonts/">
        <!--
        Wild cards can be used for defaults; any explicitly-named entry takes precedence over wildcards.
        Wild cards cannot be used to specify EmbedAction=Always; such fonts must be explicitly listed.
        Name wildcards take precedence over type wildcards.
        First in wins if duplicates exist.
        -->
        <Font type="*" name="*">
            <EmbedAction>IfUsed</EmbedAction>
        </Font>
        <Font type="TrueType" name="Arial">
            <EmbedAction>Always</EmbedAction>
        </Font>
        <Font type="RSC" name="STANDARD">
            <EmbedAction>Always</EmbedAction>
        </Font>
    </Fonts>
    <Options>
        <OptionBool name="CompactDatabase" value="true"/>
        <OptionBool name="ConvertSeedDrawingsTo3d" value="false"/>
        <OptionBool name="EmbedIssues" value="true"/>
        <OptionBool name="EmbedConfigFile" value="true"/>
        <OptionBool name="EmbedDgnLinkFiles" value="false"/>
    </Options>
</ImportConfig>
