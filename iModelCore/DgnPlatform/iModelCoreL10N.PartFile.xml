    </Part>

    <!--
        You may see that components with L10N have a series of L10N-***Xliffs-** parts, where the
        later ** indicates language name. The 'en' parts act /differently/ than all other variants;
        these parts push the xliff, but do /not/ actually generate it, unlike all other variants.
        Therefore, the 'en' (and 'pseudo') gatherers here pull in the parts that actually generate
        the 'en' xliff file, and the rest of the language gatherers are cookie-cutter.
    -->

    <Part Name="PushXliffs">
        <SubPart PartName="L10N-DgnPlatformXliffs-en" PartFile="DgnPlatform" Repository="DgnPlatform"/>
        <SubPart PartName="L10N-BeHttpXliffs-en" PartFile="BeHttp" Repository="BeHttp"/>
        <SubPart PartName="L10N-ECPresentationXliffs-en" PartFile="ECPresentation" Repository="ECPresentation"/>
    </Part>

    <Part Name="iModelCoreSqlang-en"
        BentleyBuildMakeFile="${SrcRoot}bsicommon/sqlang/mki/XliffsToDb.mke"
        BentleyBuildMakeOptions="-dSQLANG_OutputDb=$(OutputRootDir)build/iModelCoreL10N/iModelCore_en.sqlang.db3 -dSQLANG_XliffWildcard=$(BuildContext)SubParts/xliffs/*.xliff"
        >
        <SubPart PartName="DgnPlatformDLL" PartFile="DgnPlatform" Repository="DgnPlatform"/>
        <SubPart PartName="BeHttp-Xliffs" PartFile="BeHttp" Repository="BeHttp"/>
        <SubPart PartName="ECPresentation-Xliffs" PartFile="ECPresentation" Repository="ECPresentation"/>
        <Bindings>
            <Files ProductDirectoryName="Assets" ProductSubDirectory="sqlang" Required="true">Delivery/sqlang/iModelCore_en.sqlang.db3</Files>
        </Bindings>
    </Part>

    <Part Name="iModelCoreSqlang-pseudo"
        BentleyBuildMakeFile="${SrcRoot}bsicommon/sqlang/mki/XliffsToDb.mke"
        BentleyBuildMakeOptions="-dSQLANG_PseudoLocalize=Latin1 -dSQLANG_OutputDb=$(OutputRootDir)build/iModelCoreL10N/iModelCore_pseudo.sqlang.db3 -dSQLANG_XliffWildcard=$(BuildContext)SubParts/xliffs/*.xliff"
        >
        <SubPart PartName="DgnPlatformDLL" PartFile="DgnPlatform" Repository="DgnPlatform"/>
        <SubPart PartName="BeHttp-Xliffs" PartFile="BeHttp" Repository="BeHttp"/>
        <SubPart PartName="ECPresentation-Xliffs" PartFile="ECPresentation" Repository="ECPresentation"/>
        <Bindings>
