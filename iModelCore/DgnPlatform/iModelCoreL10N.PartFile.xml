        <SubPart PartName="PushXliffs"/>

        <SubPart PartName="iModelCoreSqlang-en"/>
        <SubPart PartName="iModelCoreSqlang-cs"/>
        <SubPart PartName="iModelCoreSqlang-de"/>
        <SubPart PartName="iModelCoreSqlang-es"/>
