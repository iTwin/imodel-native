<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
    <metadata>
        <id>TilePublisher</id>
        <version>$version$</version>
        <title>3D Tile Publisher</title>
        <authors>Bentley Systems, Incorporated</authors>
        <owners>Bentley Systems, Incorporated</owners>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Used to publish 3D tiles from the iModel.</description>
        <copyright>(c) $CopyRightYear$ Bentley Systems, Incorporated. All rights reserved.</copyright>
        <language>en-US</language>
        <dependencies>
            <group>
                <dependency id="DgnClientSdkAux" version="[#DgnClientSdk_version#]" />
            </group>
        </dependencies>
    </metadata>
    <files>
        <file src="Delivery\TilePublisher\scripts\**\*.*" target="Assets\scripts\" />
        <file src="PublicAPI\**\*.*"                      target="include\" />        
        <file src="VendorAPI\**\*.*"                      target="include\" />
        <file src="SubParts\Assemblies\crunchB02.dll"     target="lib\native\crunchB02.dll" />
        <file src="SubParts\Libs\crunch.lib"              target="lib\native\crunch.lib" />
        <file src="Delivery\TilePublisher.dll"            target="lib\native\TilePublisher.dll" />
        <file src="SubParts\Libs\TilePublisher.lib"       target="lib\native\TilePublisher.lib" />
        <file src="SubParts\VendorNotices\**\*.*"         target="Notices\" />
    </files>
</package>