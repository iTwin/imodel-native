<style type="text/css">
    .BentleyPlatformSampleCode
        {
        border: 1px solid #C4CFE5;
        padding: 4px 6px;
        margin: 4px 8px 4px 2px;
        overflow: auto;
        word-wrap: break-word;
        font-size:  9pt;
        line-height: 125%;
        font-family: monospace, fixed;
        font-size: 105%;
        }
</style>
<script>
        $(function()
            {
            $('a.BentleyPlatformSampleCodeOpener').click (function()
                {
                $('.' + $(this).attr ('targetsection')).toggle();

                if (this.innerText == '+')
                    this.innerText = '-';
                else
                    this.innerText = '+';
                
                this.blur();

                return false;
                });
            });
    </script>
