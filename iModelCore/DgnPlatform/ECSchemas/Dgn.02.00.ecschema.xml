    <ECClass typeName="Link" description="Base class for links." isDomainClass="True">
        <ECCustomAttributes>
            <InstanceLabelSpecification xmlns="Bentley_Standard_CustomAttributes.01.10">
                <PropertyName>DisplayLabel</PropertyName>
            </InstanceLabelSpecification>
            <ECDbClassHint xmlns="Bentley_Standard_CustomAttributes.01.10">
                <MapStrategy>TablePerHierarchy</MapStrategy>
                <ECIdColumn>LinkId</ECIdColumn>
                <ReplaceEmptyTableWithEmptyView>False</ReplaceEmptyTableWithEmptyView>
            </ECDbClassHint>
        </ECCustomAttributes>
        <ECProperty propertyName="Ordinal" typeName="int" description="Order of this node in its parent.">
            <ECCustomAttributes>
                <ECDbPropertyHint xmlns="Bentley_Standard_CustomAttributes.01.10">
                    <IsNullable>False</IsNullable>
                </ECDbPropertyHint>
            </ECCustomAttributes>
        </ECProperty>
        <ECProperty propertyName="DisplayLabel" typeName="string" description="Display label for this node." displayLabel="Display Label">
            <ECCustomAttributes>
                <ECDbPropertyHint xmlns="Bentley_Standard_CustomAttributes.01.10">
                    <IsNullable>False</IsNullable>
                </ECDbPropertyHint>
            </ECCustomAttributes>
        </ECProperty>
    </ECClass>

