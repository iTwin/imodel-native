                <TargetECInstanceIdColumn>MemberId</TargetECInstanceIdColumn>
            </LinkTableRelationshipMap>
        </ECCustomAttributes>
        <Source cardinality="(0,N)" polymorphic="True">
            <Class class="ElementGroup" />
        </Source>
        <Target cardinality="(0,N)" polymorphic="True">
            <Class class="Element" />
        </Target>
    </ECRelationshipClass>

