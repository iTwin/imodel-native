        <ECProperty propertyName="Sub1Double" typeName="double"><ECCustomAttributes><CustomHandledProperty xmlns="BisCore.01.00"/></ECCustomAttributes></ECProperty>
    </ECEntityClass>
    <ECEntityClass typeName="PerfElementCHSub2">
