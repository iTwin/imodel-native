{
  "$schema": "https://dev.bentley.com/json_schemas/ec/31/draft-01/ecschema",
  "name": "ExampleSchema",
  "version": "03.01.00",
  "alias": "ex",
  "label": "Example Schema",
  "description": "The quick brown fox jumps over the lazy dog.",
  "references": [
    {
      "name" : "CoreCustomAttributes",
      "version" : "01.00.00"
    },
    {
      "name": "refSchemaA",
      "version": "03.01.00"
    },
    {
      "name": "refSchemaB",
      "version": "03.01.00"
    }
  ],
  "customAttributes": [
    {
    "className": "refSchemaA.SomeCustomAttributeClass"
    },
    {
    "className": "refSchemaB.AnotherCustomAttributeClass"
    }
  ],
  "items": {
    "ExampleBaseEntity": {
      "schemaItemType": "EntityClass"
    },
    "ExampleEntity": {
      "schemaItemType": "EntityClass",
      "modifier": "sealed",
      "label": "ExampleEntity",
      "description": "An example entity class.",
      "baseClass": "ExampleSchema.ExampleBaseEntity",
      "mixins": [
        "ExampleSchema.ExampleMixinA",
        "ExampleSchema.ExampleMixinB"
      ]
    },
    "ExampleMixinA": {
      "schemaItemType": "Mixin",
      "appliesTo": "ExampleSchema.ExampleEntity"
    },
    "ExampleMixinB": {
      "schemaItemType": "Mixin",
      "appliesTo": "ExampleSchema.ExampleEntity"
    },
    "ExampleStruct": {
      "schemaItemType": "StructClass",
      "modifier": "sealed",
      "properties": [
        {
          "type": "PrimitiveArrayProperty",
          "name": "ExamplePrimitiveArray",
          "label": "ExPrimitiveArray",
          "typeName": "int",
          "extendedTypeName": "FooBar",
          "minValue": 7,
          "maxValue": 20,
          "minOccurs": 10,
          "maxOccurs": 25
        }
      ]
    },
    "ExampleSource": {
      "schemaItemType": "EntityClass"
    },
    "ExampleTarget": {
      "schemaItemType": "EntityClass"
    },
    "ExampleRelationship": {
      "schemaItemType": "RelationshipClass",
      "modifier": "sealed",
      "strength": "embedding",
      "strengthDirection": "forward",
      "source": {
        "multiplicity": "(0..1)",
        "roleLabel": "source roleLabel",
        "polymorphic": true,
        "constraintClasses": [
          "ExampleSchema.ExampleSource"
        ]
      },
      "target": {
        "multiplicity": "(0..1)",
        "roleLabel": "target roleLabel",
        "polymorphic": true,
        "constraintClasses": [
          "ExampleSchema.ExampleTarget"
        ]
      }
    },
    "ExampleCustomAttribute": {
        "schemaItemType": "CustomAttributeClass",
        "modifier": "sealed",
        "appliesTo": "Schema, AnyProperty"
    },
    "ExampleKoQ": {
      "schemaItemType": "KindOfQuantity",
      "persistenceUnit": {
        "unit": "MM",
        "format": "DefaultReal"
      },
      "relativeError": 3.0,
      "presentationUnits": [
        {
          "unit": "IN",
          "format": "DefaultReal"
        },
        {
          "unit": "MM",
          "format": "DefaultReal"
        },
        {
          "unit": "CM",
          "format": "DefaultReal"
        }
      ]
    },
    "ExampleEnumeration": {
      "schemaItemType": "Enumeration",
      "type": "int",
      "isStrict": true,
      "enumerators": [
        {
          "value": 1,
          "label": "None"
        },
        {
           "value": 2,
           "label": "SomeVal"
        },
        {
          "value": 3,
          "label": "AnotherVal"
        }
      ]
    },
    "ExamplePropertyCategory": {
      "schemaItemType": "PropertyCategory",
      "priority": 5
    }
  }
}