#########################################################################################
# Compile the list of tests. 
#########################################################################################
$(o)TestFixture$(oext)            : $(baseDir)scenario/TestFixture.cpp $(PCHHeaderDependency) ${MultiCompileDepends}

$(o)SchemaTests$(oext)    : $(baseDir)scenario/SchemaTests.cpp   $(PCHHeaderDependency) ${MultiCompileDepends}

$(o)InstanceTests$(oext)          : $(baseDir)scenario/InstanceTests.cpp $(PCHHeaderDependency) ${MultiCompileDepends}

$(o)ValueTests$(oext)             : $(baseDir)scenario/ValueTests.cpp $(PCHHeaderDependency) ${MultiCompileDepends}

$(o)AccessorTests$(oext)          : $(baseDir)scenario/AccessorTests.cpp $(PCHHeaderDependency) ${MultiCompileDepends}

$(o)ECRelationshipTests$(oext)    : $(baseDir)scenario/ECRelationshipTests.cpp $(PCHHeaderDependency) ${MultiCompileDepends}