MkiPublicApiDir = $(ClientPublicApiDirectory)Licensing/
MkiSourceDir = $(ClientSourceDirectory)Licensing/

#--------------------------------------------------------------------------------------
#   Set up build dependencies
#--------------------------------------------------------------------------------------
MkiHeaders =    $(MkiPublicApiDir)MobileTracking.h \
                $(MkiPublicApiDir)UsageTracking.h                     

#--------------------------------------------------------------------------------------
#   Compile
#--------------------------------------------------------------------------------------
$(o)MobileTracking$(oext)       : $(MkiSourceDir)MobileTracking.cpp $(MkiHeaders) ${MultiCompileDepends}

$(o)UsageTracking$(oext)        : $(MkiSourceDir)UsageTracking.cpp $(MkiHeaders) ${MultiCompileDepends}
