DgnDbServerClientMkiPublicApiDir = $(DgnDbServerClientPublicApiDirectory)
DgnDbServerClientMkiSourceDir = $(DgnDbServerClientSourceDirectory)DgnDbServerClient/

#----------------------------------------------------------------------------------------
#   Set up build dependencies for precompiled header and Client.mki
#----------------------------------------------------------------------------------------

DgnDbServerClientMkiHeaders =	$(DgnDbServerClientMkiPublicApiDir)DgnDbServerCommon.h						\
		                        $(DgnDbServerClientMkiPublicApiDir)Client/DgnDbBriefcase.h                  \
								$(DgnDbServerClientMkiPublicApiDir)Client/DgnDbClient.h                     \
								$(DgnDbServerClientMkiPublicApiDir)Client/DgnDbLocks.h                      \
								$(DgnDbServerClientMkiPublicApiDir)Client/DgnDbRepositoryConnection.h       \
		                        $(DgnDbServerClientMkiSourceDir)DgnDbServerUtils.h							\
		                        $(DgnDbServerClientMkiPublicApiDir)Client/RepositoryInfo.h

#----------------------------------------------------------------------------------------
#   Compile
#----------------------------------------------------------------------------------------

$(o)DgnDbServerUtils$(oext)                : $(DgnDbServerClientMkiSourceDir)DgnDbServerUtils.cpp $(DgnDbServerClientMkiHeaders) ${MultiCompileDepends}

$(o)RepositoryInfo$(oext)                  : $(DgnDbServerClientMkiSourceDir)RepositoryInfo.cpp $(DgnDbServerClientMkiHeaders) ${MultiCompileDepends}

$(o)DgnDbRepositoryConnection$(oext)       : $(DgnDbServerClientMkiSourceDir)DgnDbRepositoryConnection.cpp $(DgnDbServerClientMkiHeaders) ${MultiCompileDepends}

$(o)DgnDbBriefcase$(oext)                  : $(DgnDbServerClientMkiSourceDir)DgnDbBriefcase.cpp $(DgnDbServerClientMkiHeaders) ${MultiCompileDepends}

$(o)DgnDbLocks$(oext)                      : $(DgnDbServerClientMkiSourceDir)DgnDbLocks.cpp $(DgnDbServerClientMkiHeaders) ${MultiCompileDepends}

$(o)DgnDbClient$(oext)                     : $(DgnDbServerClientMkiSourceDir)DgnDbClient.cpp $(DgnDbServerClientMkiHeaders) ${MultiCompileDepends}

