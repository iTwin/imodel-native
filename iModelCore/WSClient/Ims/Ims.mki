ImsMkiPublicApiDir = $(ClientPublicApiDirectory)Ims/
ImsMkiSourceDir = $(ClientSourceDirectory)Ims/

#--------------------------------------------------------------------------------------
#   Set up build dependencies
#--------------------------------------------------------------------------------------
ImsMkiHeaders = $(ImsMkiPublicApiDir)SolrClient.h               \
                $(ImsMkiPublicApiDir)SolrQuery.h

#---------------------------------------------------------------------------------------+
#   Files to compile 
#---------------------------------------------------------------------------------------+
$(o)SolrClient$(oext)                               : $(ImsMkiSourceDir)SolrClient.cpp $(ImsMkiHeaders) ${MultiCompileDepends}

$(o)SolrQuery$(oext)                                : $(ImsMkiSourceDir)SolrQuery.cpp $(ImsMkiHeaders) ${MultiCompileDepends}



