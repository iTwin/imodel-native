%include mdl.mki

%if !defined (L10N_LANGUAGE_TAG)
    %error Expected L10N_LANGUAGE_TAG to be set to es, fr, etc...
%endif

xliffDir = $(SrcRoot)l10n/l10n-WSClient/$(L10N_LANGUAGE_TAG)/

$(BuildContext)Delivery/l10n/$(L10N_LANGUAGE_TAG)/WebServicesCache_$(L10N_LANGUAGE_TAG).xliff : $(xliffDir)WebServicesCache_$(L10N_LANGUAGE_TAG).xliff
    $(LinkFirstDepToFirstTarget)

$(BuildContext)Delivery/l10n/$(L10N_LANGUAGE_TAG)/WebServicesClient_$(L10N_LANGUAGE_TAG).xliff : $(xliffDir)WebServicesClient_$(L10N_LANGUAGE_TAG).xliff
    $(LinkFirstDepToFirstTarget)
