		    </ClassMap>
	  	</ECCustomAttributes>	
      <ECProperty propertyName="S" typeName="string" />
      <ECStructProperty propertyName="SS" typeName="SC" />
      <ECArrayProperty propertyName="SSA" typeName="SC" minOccurs="0" maxOccurs="unbounded" />
       
    </ECClass>
