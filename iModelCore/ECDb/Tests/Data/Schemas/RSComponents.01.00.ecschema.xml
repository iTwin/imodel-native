        <ECArrayProperty propertyName="IA_ToBeDeletedInUpgrade" typeName="long" minOccurs="0" maxOccurs="unbounded" />    
    </ECClass>  
    
    <ECClass typeName="THBase" isDomainClass="True">
		<ECCustomAttributes>
           <ClassMap xmlns="ECDbMap.01.00">
                <MapStrategy>
                    <Strategy>SharedTable</Strategy>
