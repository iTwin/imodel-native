<?xml version="1.0" encoding="utf-8"?>
<ECSchema schemaName="RSComponents" nameSpacePrefix="rsc" version="1.0" description="RS Compoents Library" displayLabel="RS Components" xmlns="http://www.bentley.com/schemas/Bentley.ECXML.2.0">
    <ECSchemaReference name="Bentley_Standard_CustomAttributes" version="01.00" prefix="bsca" />
    <ECSchemaReference name="ECDbMap" version="01.00" prefix="ecdbmap" />
    
    <ECCustomAttributes>
        <SchemaChangeTrackingState xmlns="Bentley_Standard_CustomAttributes.01.00">
            <FinalizationHistory />
            <TrackingEnabled>True</TrackingEnabled>
            <SchemaFullNameAtEditingStart>RSComponents.01.00</SchemaFullNameAtEditingStart>
        </SchemaChangeTrackingState>
    </ECCustomAttributes>
    <ECClass typeName="PCB" displayLabel="Printed Circut Board" isDomainClass="True">
        <ECProperty propertyName="NAME" typeName="string" description="Name of PCB" displayLabel="Name" />
        <ECProperty propertyName="MODEL" typeName="string" description="Model of PCB" displayLabel="Model" />
    </ECClass>
    <ECClass typeName="Addin" isDomainClass="True">
        <BaseClass>PCB</BaseClass>
        <ECProperty propertyName="VERSION" typeName="string" />
    </ECClass>
    <ECClass typeName="CAMERA" description="Camera module plug into a RaspberryPI board" displayLabel="Camera" isDomainClass="True">
        <BaseClass>Addin</BaseClass>
        <ECProperty propertyName="RESOLUTION" typeName="string" description="Camera Resolution e.g. 1024x1024" displayLabel="Resolution" />
        <ECProperty propertyName="SENSOR_TYPE" typeName="string" description="Normally CMOS or CDC" displayLabel="Sensor Type" />
        <ECProperty propertyName="COLOR_DEPTH" typeName="string" description="Color Depth of the sensor" displayLabel="Color Depth" />
    </ECClass>
    <ECClass typeName="RASPBERRYPI" description="Credit card size computer" displayLabel="RaspberryPI" isDomainClass="True">
        <BaseClass>PCB</BaseClass>
        <ECCustomAttributes>
            <BusinessKeySpecification xmlns="Bentley_Standard_CustomAttributes.01.00">
                <PropertyName>NAME</PropertyName>
            </BusinessKeySpecification>
        </ECCustomAttributes>
        <ECProperty propertyName="RAM_SIZE" typeName="string" description="Random Acces Memory on board PCB" displayLabel="RAM Size">
            <ECCustomAttributes>
                <DisplayOptions xmlns="Bentley_Standard_CustomAttributes.01.00">
                    <Hidden>False</Hidden>
                    <HideRelated>False</HideRelated>
                </DisplayOptions>
            </ECCustomAttributes>
        </ECProperty>
        <ECProperty propertyName="PROCESSOR" typeName="string" description="Processor description" displayLabel="Processor" />
        <ECProperty propertyName="ANALOG_IO_PINS" typeName="string" displayLabel="Analog IO Pins" />
        <ECProperty propertyName="DIGITAL_IO_PIN" typeName="int" displayLabel="Digital IO Pins" />
    </ECClass>
    <ECRelationshipClass typeName="RASBERRYPI_HAS_ADDIN" displayLabel="RasberryPI Has Addin" isDomainClass="True" strength="referencing" strengthDirection="forward">
        <Source cardinality="(0,1)" polymorphic="True">
            <Class class="RASPBERRYPI" />
        </Source>
        <Target cardinality="(0,N)" polymorphic="True">
            <Class class="Addin" />
        </Target>
    </ECRelationshipClass>

	<!-- Following is used with ONLY to see if ViewGeneration works for TablePerHierarchy-->
