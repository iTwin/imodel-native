<?xml version="1.0" encoding="utf-8"?>
<ECSchema schemaName="RelationshipStrengthTest" nameSpacePrefix="rst" version="1.0" xmlns="http://www.bentley.com/schemas/Bentley.ECXML.2.0">
    <ECSchemaReference name="Bentley_Standard_CustomAttributes" version="01.00" prefix="bsca" />
    <ECSchemaReference name="Bentley_Standard_Classes" version="01.00" prefix="bsc" />
    <ECClass typeName="Person" isDomainClass="True">
        <ECProperty propertyName="FirstName" typeName="string" displayLabel="First Name" />
        <ECProperty propertyName="LastName" typeName="string" displayLabel="Last Name"/>
    </ECClass>

    <ECRelationshipClass typeName="SingleParentHasChildren" isDomainClass="True" strength="embedding" strengthDirection="forward">
        <Source cardinality="(1,1)" polymorphic="True" roleLabel="Parent">
            <Class class="Person" />
        </Source>
        <Target cardinality="(0,N)" polymorphic="True" roleLabel="Children">
            <Class class="Person" />
        </Target>
    </ECRelationshipClass>

    <ECRelationshipClass typeName="ManyParentsHaveChildren" isDomainClass="True" strength="holding" strengthDirection="forward">
        <Source cardinality="(1,N)" polymorphic="True" roleLabel="Parents">
            <Class class="Person" />
        </Source>
        <Target cardinality="(0,N)" polymorphic="True" roleLabel="Children">
            <Class class="Person" />
        </Target>
    </ECRelationshipClass>

    <ECRelationshipClass typeName="ParentHasSpouse" isDomainClass="True" strength="referencing" strengthDirection="forward">
        <Source cardinality="(0,1)" polymorphic="True" roleLabel="Parent">
            <Class class="Person" />
        </Source>
        <Target cardinality="(0,1)" polymorphic="True" roleLabel="Spouse">
            <Class class="Person" />
        </Target>
    </ECRelationshipClass>

    <ECRelationshipClass typeName="ParentHasAnyChild" isDomainClass="True" strength="embedding" strengthDirection="forward">
        <Source cardinality="(1,1)" polymorphic="True" roleLabel="Parent">
            <Class class="Person" />
        </Source>
        <Target cardinality="(0,N)" polymorphic="True" roleLabel="Children">
            <Class class="bsc:AnyClass" />
        </Target>
    </ECRelationshipClass>

    <ECRelationshipClass typeName="ParentsHaveAnyChildren" isDomainClass="True" strength="holding" strengthDirection="forward">
        <Source cardinality="(1,N)" polymorphic="True" roleLabel="Parent">
            <Class class="Person" />
        </Source>
        <Target cardinality="(0,N)" polymorphic="True" roleLabel="Children">
            <Class class="bsc:AnyClass" />
        </Target>
    </ECRelationshipClass>

</ECSchema>
