<?xml version="1.0" encoding="utf-8"?>
<ECSchema schemaName="ECSqlStatementTests" nameSpacePrefix="ECST" version="1.0" displayLabel="ECSqlStatementTests DataBase" xmlns="http://www.bentley.com/schemas/Bentley.ECXML.2.0">
    <ECSchemaReference name="Bentley_Standard_CustomAttributes" version="01.00" prefix="bsca" />
    <ECCustomAttributes>
        <SchemaChangeTrackingState xmlns="Bentley_Standard_CustomAttributes.01.00">
            <FinalizationHistory />
            <TrackingEnabled>True</TrackingEnabled>
            <SchemaFullNameAtEditingStart>ECSqlStatementTests.01.00</SchemaFullNameAtEditingStart>
        </SchemaChangeTrackingState>
    </ECCustomAttributes>
    <ECClass typeName="ContactDetails" isStruct="True">
        <ECProperty propertyName="ContactType" typeName="string" />
        <ECProperty propertyName="Notes" typeName="string" />
    </ECClass>
    <ECClass typeName="Name" isStruct="True">
        <ECProperty propertyName="FirstName" typeName="string" />
        <ECProperty propertyName="LastName" typeName="string" />
    </ECClass>
    <ECClass typeName="Person" isDomainClass="False">
        <ECProperty propertyName="Phone" typeName="long" />
        <ECProperty propertyName="City" typeName="string" />
        <ECProperty propertyName="Country" typeName="string" />
        <ECProperty propertyName="Address" typeName="string" />
        <ECStructProperty propertyName="PersonName" typeName="Name" />
    </ECClass>
    <ECClass typeName="Customer" isDomainClass="True">
        <BaseClass>Person</BaseClass>
        <ECProperty propertyName="ContactTitle" typeName="string" />
        <ECProperty propertyName="Company" typeName="string" />
        <ECProperty propertyName="IsRegular" typeName="boolean" />
        <ECArrayProperty propertyName="ContactDetails" typeName="ContactDetails" minOccurs="1" maxOccurs="5" isStruct="True" />
    </ECClass>
    <ECClass typeName="Order" isDomainClass="True">
        <ECProperty propertyName="OrderDate" typeName="string" />
		<ECProperty propertyName="ReleaseDate" typeName="string" />
		<ECProperty propertyName="ShipCity" typeName="string" />
		<ECProperty propertyName="ShipPostalCode" typeName="string" />
    </ECClass>
    <ECRelationshipClass typeName="CustomerHasOrder" isDomainClass="True" strength="referencing" strengthDirection="forward">
        <Source cardinality="(0,1)" polymorphic="True">
            <Class class="Customer" />
        </Source>
        <Target cardinality="(1,N)" polymorphic="True">
            <Class class="Order" />
        </Target>
    </ECRelationshipClass>
    <ECClass typeName="Employee" isDomainClass="True">
        <BaseClass>Person</BaseClass>
        <ECProperty propertyName="IsContractual" typeName="boolean" />
        <ECArrayProperty propertyName="ContactDetails" typeName="ContactDetails" minOccurs="1" maxOccurs="10" isStruct="True" />
    </ECClass>
    <ECRelationshipClass typeName="EmployeeHasOrder" isDomainClass="True" strength="referencing" strengthDirection="forward">
        <Source cardinality="(0,1)" roleLabel="EmployeeHasOrder" polymorphic="True">
            <Class class="Employee" />
        </Source>
        <Target cardinality="(0,N)" polymorphic="True">
            <Class class="Order" />
        </Target>
    </ECRelationshipClass>
    <ECClass typeName="Product" isDomainClass="True">
        <ECProperty propertyName="ProductName" typeName="string" />
        <ECProperty propertyName="UnitPrice" typeName="double" />
        <ECProperty propertyName="ProductAvailable" typeName="boolean" />
    </ECClass>
    <ECClass typeName="Shipper" isDomainClass="True">
        <ECProperty propertyName="CompanyName" typeName="string" />
        <ECProperty propertyName="Phone" typeName="long" />
    </ECClass>
    <ECClass typeName="Supplier" isDomainClass="True">
        <ECProperty propertyName="CompanyName" typeName="string" />
        <ECProperty propertyName="ContactName" typeName="string" />
        <ECProperty propertyName="ContactTitle" typeName="string" />
        <ECProperty propertyName="Address" typeName="string" />
        <ECProperty propertyName="City" typeName="string" />
        <ECProperty propertyName="Country" typeName="string" />
        <ECProperty propertyName="Phone" typeName="long" />
    </ECClass>
	    <ECRelationshipClass typeName="ProductHasSupplier" isDomainClass="True" strength="referencing" strengthDirection="forward">
        <Source cardinality="(0,1)" roleLabel="ProductHasSupplier" polymorphic="True">
            <Class class="Product" />
        </Source>
        <Target cardinality="(0,N)" polymorphic="True">
            <Class class="Supplier" />
        </Target>
    </ECRelationshipClass>
</ECSchema>