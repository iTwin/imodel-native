<?xml version="1.0" encoding="UTF-8"?>
<ECSchema schemaName="Computers" alias="TR" version="01.00" xmlns="http://www.bentley.com/schemas/Bentley.ECXML.3.1">
    <ECEntityClass typeName="Desktop" modifier="None">
        <ECProperty propertyName="OS" typeName="string" readOnly="false"/>
    </ECEntityClass>
    <ECEntityClass typeName="Laptop" modifier="None">
        <ECProperty propertyName="OS" typeName="string" readOnly="false"/>
    </ECEntityClass>
    <ECRelationshipClass typeName="LaptopHasLaptop" modifier="Sealed" strength="referencing" strengthDirection="forward">
        <Source multiplicity="(0..1)" polymorphic="true" roleLabel="holds">
            <Class class="Laptop"/>
        </Source>
        <Target multiplicity="(0..1)" polymorphic="true" roleLabel="is held by">
            <Class class="Laptop"/>
        </Target>
    </ECRelationshipClass>
    <ECEntityClass typeName="RAM" modifier="None">
        <ECProperty propertyName="Size" typeName="string" readOnly="false"/>
    </ECEntityClass>
    <ECRelationshipClass typeName="LaptopHasRam" modifier="Sealed" strength="referencing" strengthDirection="forward">
        <Source multiplicity="(0..1)" polymorphic="true" roleLabel="has">
            <Class class="Laptop"/>
        </Source>
        <Target multiplicity="(0..1)" polymorphic="true" roleLabel="is built into">
            <Class class="RAM"/>
        </Target>
    </ECRelationshipClass>
    <ECRelationshipClass typeName="LaptopHaveManyRams" modifier="Sealed" strength="referencing" strengthDirection="forward">
        <ECProperty propertyName="price" typeName="int" readOnly="false"/>
        <Source multiplicity="(0..1)" polymorphic="true" roleLabel="has">
            <Class class="Laptop"/>
        </Source>
        <Target multiplicity="(0..*)" polymorphic="true" roleLabel="is built into">
            <Class class="RAM"/>
        </Target>
    </ECRelationshipClass>
    <ECRelationshipClass typeName="ManyLaptopsHaveManyRams" modifier="Sealed" strength="referencing" strengthDirection="forward">
        <Source multiplicity="(0..*)" polymorphic="true" roleLabel="has">
            <Class class="Laptop"/>
        </Source>
        <Target multiplicity="(0..*)" polymorphic="true" roleLabel="is built into">
            <Class class="RAM"/>
        </Target>
    </ECRelationshipClass>
</ECSchema>
