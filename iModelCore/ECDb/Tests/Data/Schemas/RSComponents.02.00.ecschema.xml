<?xml version="1.0" encoding="UTF-8"?>
<ECSchema schemaName="RSComponents" nameSpacePrefix="rsc" version="02.00" description="RS Compoents Library 1.0" displayLabel="RS Components Library" xmlns="http://www.bentley.com/schemas/Bentley.ECXML.3.0">
    <ECSchemaReference name="Bentley_Standard_CustomAttributes" version="01.12" prefix="bsca"/>
    <ECSchemaReference name="EditorCustomAttributes" version="01.02" prefix="beca"/>
    <ECSchemaReference name="ECDbMap" version="02.00" prefix="ecdbmap"/>
    <ECCustomAttributes>
        <SchemaChangeTrackingState xmlns="Bentley_Standard_CustomAttributes.01.12">
            <SchemaFullNameAtEditingStart>RSComponents.01.00</SchemaFullNameAtEditingStart>
            <TrackingEnabled>True</TrackingEnabled>
        </SchemaChangeTrackingState>
    </ECCustomAttributes>
    <ECEntityClass typeName="PCB" description="RS Components PCB Board in production" displayLabel="Printed Circut Board" modifier="None">
        <ECProperty propertyName="NAME" typeName="string" description="Name of PCB" displayLabel="Name" readOnly="false"/>
        <ECProperty propertyName="MODEL" typeName="string" description="Model of PCB" displayLabel="Model" readOnly="false"/>
        <ECProperty propertyName="PRICE" typeName="string" description="Menufacturing price of the PCB board" displayLabel="Price" readOnly="false"/>
    </ECEntityClass>
    <ECEntityClass typeName="Addin" description="Raspberry PI Addin PCB Board" displayLabel="Add-In" modifier="None">
        <BaseClass>PCB</BaseClass>
        <ECProperty propertyName="VERSION" typeName="string" displayLabel="Version" readOnly="false"/>
        <ECArrayProperty propertyName="SUPPORTED_MODELS" typeName="string" description="List of supported raspberry models" displayLabel="Supported Models" readOnly="false" minOccurs="0" maxOccurs="unbounded"/>
    </ECEntityClass>
    <ECEntityClass typeName="CAMERA" description="Camera module plug into a RaspberryPI board" displayLabel="Camera Addin" modifier="None">
        <BaseClass>Addin</BaseClass>
        <ECProperty propertyName="RESOLUTION" typeName="string" description="Camera Resolution e.g. 3024x3024" displayLabel="Resolution" readOnly="false"/>
        <ECProperty propertyName="SENSOR_TYPE" typeName="string" description="Normally its CMOS or CDC" displayLabel="Sensor Type" readOnly="false"/>
        <ECProperty propertyName="COLOR_DEPTH" typeName="string" description="Color Depth of the sensor 16bit, 24bit." displayLabel="Color Resolution" readOnly="false"/>
    </ECEntityClass>
    <ECEntityClass typeName="RASPBERRYPI" description="Credit card size computer" displayLabel="Raspberry PI" modifier="None">
        <BaseClass>PCB</BaseClass>
        <ECCustomAttributes>
            <BusinessKeySpecification xmlns="Bentley_Standard_CustomAttributes.01.12">
                <PropertyName>Name</PropertyName>
            </BusinessKeySpecification>
            <Category xmlns="EditorCustomAttributes.01.02">
                <DisplayLabel>RespBerryPI</DisplayLabel>
                <Expand>True</Expand>
            </Category>
        </ECCustomAttributes>
        <ECProperty propertyName="RAM_SIZE" typeName="string" description="Random Access Memory on board PCB" displayLabel="RAM Size" readOnly="false">
            <ECCustomAttributes>
                <DisplayOptions xmlns="Bentley_Standard_CustomAttributes.01.12">
                    <Hidden>False</Hidden>
                    <HideRelated>False</HideRelated>
                </DisplayOptions>
            </ECCustomAttributes>
        </ECProperty>
        <ECProperty propertyName="PROCESSOR" typeName="string" description="Processor description" displayLabel="Processor" readOnly="false"/>
        <ECProperty propertyName="ANALOG_IO_PINS" typeName="string" displayLabel="Analog IO Pins" readOnly="false"/>
        <ECProperty propertyName="DIGITAL_IO_PIN" typeName="int" displayLabel="Digital IO Pins" readOnly="false"/>
        <ECProperty propertyName="USB_VER" typeName="string" description="USB version supported." displayLabel="USB version" readOnly="false">
            <ECCustomAttributes>
                <BooleanDisplay xmlns="EditorCustomAttributes.01.02">
                    <TrueString>True</TrueString>
                    <FalseString>False</FalseString>
                </BooleanDisplay>
            </ECCustomAttributes>
        </ECProperty>
    </ECEntityClass>
    <ECRelationshipClass typeName="RASBERRYPI_HAS_ADDIN" displayLabel="RasberryPI Has Add-in" modifier="Sealed" strength="holding" strengthDirection="backward">
        <ECProperty propertyName="COMPITABLE" typeName="boolean" displayLabel="Compitable" readOnly="false"/>
        <Source cardinality="(0,1)" roleLabel="ROLE_LABEL" polymorphic="true">
            <Class class="RASPBERRYPI"/>
        </Source>
        <Target cardinality="(0,N)" roleLabel="ROLE_LABEL (REV)" polymorphic="true">
            <Class class="Addin"/>
        </Target>
    </ECRelationshipClass>
    <ECStructClass typeName="SC" modifier="None">
        <ECProperty propertyName="I" typeName="long" readOnly="false"/>
    </ECStructClass>
    <ECEntityClass typeName="SONOR" description="Sonor Sensor to detect a object at a distance using ultarsonic sound" displayLabel="Sonor Addin" modifier="None">
        <BaseClass>Addin</BaseClass>
        <ECProperty propertyName="RESEVER_SENSITIVITY" typeName="string" description="Specified in +-db" displayLabel="Reseer Sensitivity" readOnly="false"/>
        <ECProperty propertyName="MAX_DISTANCE" typeName="int" description="Max distance in cm where object can be detected in clear line of sight" displayLabel="Max Distance" readOnly="false"/>
    </ECEntityClass>
    <ECEntityClass typeName="TCBase" modifier="None">
        <ECProperty propertyName="S" typeName="string" readOnly="false"/>
        <ECStructProperty propertyName="SS" typeName="SC" readOnly="false"/>
        <ECStructArrayProperty propertyName="SSA" typeName="SC" readOnly="false" minOccurs="0" maxOccurs="unbounded"/>
    </ECEntityClass>
    <ECEntityClass typeName="TC1" modifier="None">
        <BaseClass>TCBase</BaseClass>
        <ECProperty propertyName="S1" typeName="string" readOnly="false"/>
    </ECEntityClass>
    <ECEntityClass typeName="TC2" modifier="None">
        <BaseClass>TC1</BaseClass>
        <ECProperty propertyName="S2" typeName="string" readOnly="false"/>
    </ECEntityClass>
    <ECEntityClass typeName="TC3" modifier="None">
        <BaseClass>TC2</BaseClass>
        <ECProperty propertyName="S3" typeName="string" readOnly="false"/>
    </ECEntityClass>
    <ECEntityClass typeName="THBase" modifier="None">
        <ECCustomAttributes>
            <ClassMap xmlns="ECDbMap.02.00">
                <MapStrategy>TablePerHierarchy</MapStrategy>
            </ClassMap>
        </ECCustomAttributes>
        <ECProperty propertyName="S" typeName="string" readOnly="false"/>
        <ECStructProperty propertyName="SS" typeName="SC" readOnly="false"/>
        <ECStructArrayProperty propertyName="SSA" typeName="SC" readOnly="false" minOccurs="0" maxOccurs="unbounded"/>
    </ECEntityClass>
    <ECEntityClass typeName="TH1" modifier="None">
        <BaseClass>THBase</BaseClass>
        <ECProperty propertyName="S1" typeName="string" readOnly="false"/>
    </ECEntityClass>
    <ECEntityClass typeName="TH2" modifier="None">
        <BaseClass>TH1</BaseClass>
        <ECProperty propertyName="S2" typeName="string" readOnly="false"/>
    </ECEntityClass>
    <ECEntityClass typeName="TH3" modifier="None">
        <BaseClass>TH2</BaseClass>
        <ECProperty propertyName="S3" typeName="string" readOnly="false"/>
    </ECEntityClass>
</ECSchema>
