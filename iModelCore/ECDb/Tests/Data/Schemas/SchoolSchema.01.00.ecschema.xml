<?xml version="1.0" encoding="UTF-8"?>
<ECSchema schemaName="SchoolSchema" nameSpacePrefix="SS" version="01.00" xmlns="http://www.bentley.com/schemas/Bentley.ECXML.3.0">
    <ECSchemaReference name="Bentley_Standard_CustomAttributes" version="01.12" prefix="bsca"/>
    <ECSchemaReference name="EditorCustomAttributes" version="01.02" prefix="beca"/>
    <ECSchemaReference name="Unit_Attributes" version="01.00" prefix="units_attribs"/>
    <ECCustomAttributes>
        <SchemaChangeTrackingState xmlns="Bentley_Standard_CustomAttributes.01.12">
            <SchemaFullNameAtEditingStart>SchoolSchema.01.00</SchemaFullNameAtEditingStart>
            <TrackingEnabled>True</TrackingEnabled>
        </SchemaChangeTrackingState>
    </ECCustomAttributes>
    <ECEntityClass typeName="Course" modifier="None">
        <ECCustomAttributes>
            <TreeColorCustomAttributes xmlns="Bentley_Standard_CustomAttributes.01.12">
                <NodeBackColor>ActiveBorder</NodeBackColor>
                <NodeForeColor>ActiveCaption</NodeForeColor>
            </TreeColorCustomAttributes>
        </ECCustomAttributes>
    </ECEntityClass>
    <ECEntityClass typeName="CourseTitle" modifier="None">
        <BaseClass>Course</BaseClass>
        <ECCustomAttributes>
            <DisplayOptions xmlns="Bentley_Standard_CustomAttributes.01.12">
                <Hidden>False</Hidden>
                <HideRelated>False</HideRelated>
            </DisplayOptions>
        </ECCustomAttributes>
    </ECEntityClass>
    <ECEntityClass typeName="Department" modifier="None">
        <ECCustomAttributes>
            <CEContentPaneCustomization xmlns="Bentley_Standard_CustomAttributes.01.12"/>
            <ChangeManagement xmlns="Bentley_Standard_CustomAttributes.01.12">
                <Supported>True</Supported>
            </ChangeManagement>
        </ECCustomAttributes>
        <ECProperty propertyName="d_code" typeName="string" readOnly="false"/>
        <ECProperty propertyName="school_code" typeName="string" readOnly="false"/>
        <ECProperty propertyName="d_name" typeName="string" readOnly="false"/>
    </ECEntityClass>
    <ECEntityClass typeName="School" modifier="None">
        <ECCustomAttributes>
            <DateTimeInfo xmlns="Bentley_Standard_CustomAttributes.01.12"/>
        </ECCustomAttributes>
        <ECProperty propertyName="code" typeName="string" readOnly="false"/>
        <ECProperty propertyName="name" typeName="string" readOnly="false"/>
        <ECProperty propertyName="campus" typeName="string" readOnly="false"/>
    </ECEntityClass>
    <ECRelationshipClass typeName="SchoolDepartmentRelation" modifier="Sealed" strength="referencing" strengthDirection="forward">
        <ECCustomAttributes>
            <AlwaysExpand xmlns="EditorCustomAttributes.01.02">
                <ArrayMembers>False</ArrayMembers>
            </AlwaysExpand>
            <BooleanDisplay xmlns="EditorCustomAttributes.01.02">
                <TrueString>yes</TrueString>
                <FalseString>no</FalseString>
            </BooleanDisplay>
            <TreeColorCustomAttributes xmlns="Bentley_Standard_CustomAttributes.01.12">
                <NodeBackColor>ActiveBorder</NodeBackColor>
                <NodeForeColor>ActiveCaption</NodeForeColor>
            </TreeColorCustomAttributes>
        </ECCustomAttributes>
        <Source cardinality="(0,1)" polymorphic="true">
            <Class class="School"/>
        </Source>
        <Target cardinality="(0,N)" polymorphic="true">
            <Class class="Department"/>
        </Target>
    </ECRelationshipClass>
</ECSchema>
