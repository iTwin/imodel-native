<?xml version="1.0" encoding="UTF-8"?>
<ECSchema schemaName="DWGR19_L0" nameSpacePrefix="dwg0" version="01.01" xmlns="http://www.bentley.com/schemas/Bentley.ECXML.3.0">
    <ECSchemaReference name="Bentley_Standard_CustomAttributes" version="01.12" prefix="bsca"/>
    <ECSchemaReference name="EditorCustomAttributes" version="01.02" prefix="beca"/>
    <ECEntityClass typeName="AcRxClass" description="Object class definition" displayLabel="Runtime Class" modifier="None">
        <ECCustomAttributes>
            <InstanceLabelSpecification xmlns="Bentley_Standard_CustomAttributes.01.12">
                <PropertyName>name</PropertyName>
            </InstanceLabelSpecification>
        </ECCustomAttributes>
        <ECProperty propertyName="name" typeName="string" displayLabel="Type Name" readOnly="false">
            <ECCustomAttributes>
                <Category xmlns="EditorCustomAttributes.01.02">
                    <Name>Information</Name>
                </Category>
            </ECCustomAttributes>
        </ECProperty>
        <ECProperty propertyName="dxfName" typeName="string" displayLabel="Drawing Exchange Format" readOnly="false">
            <ECCustomAttributes>
                <Category xmlns="EditorCustomAttributes.01.02">
                    <Name>Information</Name>
                </Category>
            </ECCustomAttributes>
        </ECProperty>
        <ECProperty propertyName="appName" typeName="string" displayLabel="Application" readOnly="false">
            <ECCustomAttributes>
                <Category xmlns="EditorCustomAttributes.01.02">
                    <Name>Information</Name>
                </Category>
            </ECCustomAttributes>
        </ECProperty>
    </ECEntityClass>
    <ECEntityClass typeName="AcRxObject" displayLabel="Object" modifier="None">
        <ECCustomAttributes>
            <SyncIDSpecification xmlns="Bentley_Standard_CustomAttributes.01.12">
                <Property>idHandle</Property>
            </SyncIDSpecification>
        </ECCustomAttributes>
        <ECProperty propertyName="idHandle" typeName="string" description="Original object handle" displayLabel="Handle" readOnly="false">
            <ECCustomAttributes>
                <Category xmlns="EditorCustomAttributes.01.02">
                    <Name>Extended</Name>
                </Category>
            </ECCustomAttributes>
        </ECProperty>
    </ECEntityClass>
    <ECCustomAttributeClass typeName="MappingStrategy" displayLabel="Mapping Strategy" modifier="None" appliesTo="Any">
        <ECProperty propertyName="MethodName" typeName="string" displayLabel="Method Name" readOnly="false"/>
    </ECCustomAttributeClass>
    <ECRelationshipClass typeName="ObjectHasDefinition" modifier="Sealed" strength="referencing" strengthDirection="backward">
        <Source cardinality="(1,N)" roleLabel="Defined by" polymorphic="true">
            <Class class="AcRxObject"/>
        </Source>
        <Target cardinality="(1,1)" roleLabel="Has Instances" polymorphic="false">
            <Class class="AcRxClass"/>
        </Target>
    </ECRelationshipClass>
</ECSchema>
