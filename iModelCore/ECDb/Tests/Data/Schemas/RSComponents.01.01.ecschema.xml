        <ECArrayProperty propertyName="SA_3_ToBeDeletedInUpgrade" typeName="string" minOccurs="0" maxOccurs="unbounded" />
        <ECStructProperty propertyName="SS3_ToBeDeletedInUpgrade" typeName="SC" />
        <ECArrayProperty propertyName="SSA3_ToBeDeletedInUpgrade" typeName="SC" minOccurs="0" maxOccurs="unbounded" isStruct="True" />
    </ECClass>
    <ECClass typeName="TestClass" isDomainClass="True" />
    <ECClass typeName="THBase" isDomainClass="True">
        <ECCustomAttributes>
           <ClassMap xmlns="ECDbMap.01.00">
                <MapStrategy>
                    <Strategy>SharedTable</Strategy>
                    <AppliesToSubclasses>True</AppliesToSubclasses>
                </MapStrategy>
		    </ClassMap>
        </ECCustomAttributes>
        <ECProperty propertyName="S" typeName="string" />
        <ECStructProperty propertyName="SS" typeName="SC" />
        <ECArrayProperty propertyName="SSA" typeName="SC" minOccurs="0" maxOccurs="unbounded" isStruct="True" />
    </ECClass>
    <ECClass typeName="TH1" isDomainClass="True">
        <BaseClass>THBase</BaseClass>
