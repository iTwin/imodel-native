<?xml version="1.0" encoding="UTF-8"?>
<ECSchema schemaName="RSComponents" nameSpacePrefix="rsc" version="01.01" description="RS Compoents Library" xmlns="http://www.bentley.com/schemas/Bentley.ECXML.3.0">
    <ECSchemaReference name="Bentley_Standard_CustomAttributes" version="01.12" prefix="bsca"/>
    <ECSchemaReference name="ECDbMap" version="01.00.01" prefix="ecdbmap"/>
    <ECCustomAttributes>
        <SchemaChangeTrackingState xmlns="Bentley_Standard_CustomAttributes.01.12">
            <SchemaFullNameAtEditingStart>RSComponents.01.01</SchemaFullNameAtEditingStart>
            <TrackingEnabled>True</TrackingEnabled>
        </SchemaChangeTrackingState>
    </ECCustomAttributes>
    <ECEntityClass typeName="PCB" displayLabel="Printed Circut Board" modifier="None">
        <ECProperty propertyName="NAME" typeName="string" description="Name of PCB" displayLabel="Name" readOnly="false"/>
        <ECProperty propertyName="MODEL" typeName="string" description="Model of PCB" displayLabel="Model" readOnly="false"/>
        <ECProperty propertyName="PRICE" typeName="string" description="Manufacturing price of the PCB board" displayLabel="price" readOnly="false"/>
    </ECEntityClass>
    <ECEntityClass typeName="Addin" displayLabel="Add-In" modifier="None">
        <BaseClass>PCB</BaseClass>
        <ECProperty propertyName="VERSION" typeName="string" readOnly="false"/>
        <ECArrayProperty propertyName="SUPPORTED_MODELS" typeName="string" description="List of supported respberry models" displayLabel="Supported Models" readOnly="false" minOccurs="0" maxOccurs="unbounded"/>
    </ECEntityClass>
    <ECEntityClass typeName="CAMERA" description="Camera module plug into a RaspberryPI board" displayLabel="Camera Addin" modifier="None">
        <BaseClass>Addin</BaseClass>
        <ECProperty propertyName="RESOLUTION" typeName="string" description="Camera Resolution e.g. 1024x1024" displayLabel="Resolution" readOnly="false"/>
        <ECProperty propertyName="SENSOR_TYPE" typeName="string" description="Normally CMOS or CDC" displayLabel="Sensor Type" readOnly="false"/>
        <ECProperty propertyName="COLOR_DEPTH" typeName="string" description="Color Depth of the sensor" displayLabel="Color Depth" readOnly="false"/>
    </ECEntityClass>
    <ECEntityClass typeName="RASPBERRYPI" description="Credit card size computer" displayLabel="RaspberryPI" modifier="None">
        <BaseClass>PCB</BaseClass>
        <ECCustomAttributes>
            <BusinessKeySpecification xmlns="Bentley_Standard_CustomAttributes.01.12">
                <PropertyName>NAME</PropertyName>
            </BusinessKeySpecification>
        </ECCustomAttributes>
        <ECProperty propertyName="RAM_SIZE" typeName="string" description="Random Acces Memory on board PCB" displayLabel="RAM Size" readOnly="false">
            <ECCustomAttributes>
                <DisplayOptions xmlns="Bentley_Standard_CustomAttributes.01.12">
                    <Hidden>False</Hidden>
                    <HideRelated>False</HideRelated>
                </DisplayOptions>
            </ECCustomAttributes>
        </ECProperty>
        <ECProperty propertyName="PROCESSOR" typeName="string" description="Processor description" displayLabel="Processor" readOnly="false"/>
        <ECProperty propertyName="ANALOG_IO_PINS" typeName="string" displayLabel="Analog IO Pins" readOnly="false"/>
        <ECProperty propertyName="DIGITAL_IO_PIN" typeName="int" displayLabel="Digital IO Pins" readOnly="false"/>
        <ECProperty propertyName="USE_VER" typeName="string" description="usb version supported." displayLabel="USB version" readOnly="false"/>
    </ECEntityClass>
    <ECRelationshipClass typeName="RASBERRYPI_HAS_ADDIN" displayLabel="RasberryPI Has Addin" modifier="Sealed" strength="referencing" strengthDirection="forward">
        <Source cardinality="(0,1)" polymorphic="true">
            <Class class="RASPBERRYPI"/>
        </Source>
        <Target cardinality="(0,N)" polymorphic="true">
            <Class class="Addin"/>
        </Target>
    </ECRelationshipClass>
    <ECStructClass typeName="SC" modifier="None">
        <ECProperty propertyName="I" typeName="long" readOnly="false"/>
    </ECStructClass>
    <ECEntityClass typeName="SONOR" description="Sonor Sensor to detect a obhect at a distance using ultrasonic sound" displayLabel="Sonor Addin" modifier="None">
        <BaseClass>Addin</BaseClass>
        <ECProperty propertyName="RESERVER_SENSITIVITY" typeName="string" description="Specified in db" displayLabel="Reserver Sensitivity" readOnly="false"/>
        <ECProperty propertyName="MAX_DISTANCE" typeName="int" description="Max distance in cm where objcet can be detected in clear line of sight" displayLabel="Max Distance" readOnly="false"/>
    </ECEntityClass>
    <ECEntityClass typeName="TCBase" modifier="None">
        <ECProperty propertyName="S" typeName="string" readOnly="false"/>
        <ECStructProperty propertyName="SS" typeName="SC" readOnly="false"/>
        <ECStructArrayProperty propertyName="SSA" typeName="SC" readOnly="false" minOccurs="0" maxOccurs="unbounded"/>
    </ECEntityClass>
    <ECEntityClass typeName="TC1" modifier="None">
        <BaseClass>TCBase</BaseClass>
        <ECProperty propertyName="S" typeName="string" readOnly="false"/>
        <ECProperty propertyName="S1" typeName="string" readOnly="false"/>
        <ECProperty propertyName="S1_ToBeDeletedInUpgrade" typeName="string" readOnly="false"/>
        <ECArrayProperty propertyName="SA_1_ToBeDeletedInUpgrade" typeName="string" readOnly="false" minOccurs="0" maxOccurs="unbounded"/>
        <ECStructProperty propertyName="SS1_ToBeDeletedInUpgrade" typeName="SC" readOnly="false"/>
        <ECStructArrayProperty propertyName="SSA1_ToBeDeletedInUpgrade" typeName="SC" readOnly="false" minOccurs="0" maxOccurs="unbounded"/>
    </ECEntityClass>
    <ECEntityClass typeName="TC2" modifier="None">
        <BaseClass>TC1</BaseClass>
        <ECProperty propertyName="S" typeName="string" readOnly="false"/>
        <ECProperty propertyName="S2" typeName="string" readOnly="false"/>
        <ECProperty propertyName="S2_ToBeDeletedInUpgrade" typeName="string" readOnly="false"/>
        <ECArrayProperty propertyName="SA_2_ToBeDeletedInUpgrade" typeName="string" readOnly="false" minOccurs="0" maxOccurs="unbounded"/>
        <ECStructProperty propertyName="SS3_ToBeDeletedInUpgrade" typeName="SC" readOnly="false"/>
        <ECStructArrayProperty propertyName="SSA3_ToBeDeletedInUpgrade" typeName="SC" readOnly="false" minOccurs="0" maxOccurs="unbounded"/>
    </ECEntityClass>
    <ECEntityClass typeName="TC3" modifier="None">
        <BaseClass>TC2</BaseClass>
        <ECProperty propertyName="S" typeName="string" readOnly="false"/>
        <ECProperty propertyName="S3" typeName="string" readOnly="false"/>
        <ECProperty propertyName="S3_ToBeDeletedInUpgrade" typeName="string" readOnly="false"/>
        <ECArrayProperty propertyName="SA_3_ToBeDeletedInUpgrade" typeName="string" readOnly="false" minOccurs="0" maxOccurs="unbounded"/>
        <ECStructProperty propertyName="SS3_ToBeDeletedInUpgrade" typeName="SC" readOnly="false"/>
        <ECStructArrayProperty propertyName="SSA3_ToBeDeletedInUpgrade" typeName="SC" readOnly="false" minOccurs="0" maxOccurs="unbounded"/>
    </ECEntityClass>
    <ECEntityClass typeName="THBase" modifier="None">
        <ECCustomAttributes>
            <ClassMap xmlns="ECDbMap.01.00.01">
                <MapStrategy>
                    <Strategy>SharedTable</Strategy>
                    <AppliesToSubclasses>True</AppliesToSubclasses>
                </MapStrategy>
            </ClassMap>
        </ECCustomAttributes>
        <ECProperty propertyName="S" typeName="string" readOnly="false"/>
        <ECStructProperty propertyName="SS" typeName="SC" readOnly="false"/>
        <ECStructArrayProperty propertyName="SSA" typeName="SC" readOnly="false" minOccurs="0" maxOccurs="unbounded"/>
    </ECEntityClass>
    <ECEntityClass typeName="TH1" modifier="None">
        <BaseClass>THBase</BaseClass>
        <ECProperty propertyName="S" typeName="string" readOnly="false"/>
        <ECProperty propertyName="S1" typeName="string" readOnly="false"/>
        <ECProperty propertyName="S1_ToBeDeletedInUpgrade" typeName="string" readOnly="false"/>
        <ECArrayProperty propertyName="SA_1_ToBeDeletedInUpgrade" typeName="string" readOnly="false" minOccurs="0" maxOccurs="unbounded"/>
        <ECStructProperty propertyName="SS1_ToBeDeletedInUpgrade" typeName="SC" readOnly="false"/>
        <ECStructArrayProperty propertyName="SSA1_ToBeDeletedInUpgrade" typeName="SC" readOnly="false" minOccurs="0" maxOccurs="unbounded"/>
    </ECEntityClass>
    <ECEntityClass typeName="TH2" modifier="None">
        <BaseClass>TH1</BaseClass>
        <ECProperty propertyName="S" typeName="string" readOnly="false"/>
        <ECProperty propertyName="S2" typeName="string" readOnly="false"/>
        <ECProperty propertyName="S2_ToBeDeletedInUpgrade" typeName="string" readOnly="false"/>
        <ECArrayProperty propertyName="SA_2_ToBeDeletedInUpgrade" typeName="string" readOnly="false" minOccurs="0" maxOccurs="unbounded"/>
        <ECStructProperty propertyName="SS2_ToBeDeletedInUpgrade" typeName="SC" readOnly="false"/>
        <ECStructArrayProperty propertyName="SSA2_ToBeDeletedInUpgrade" typeName="SC" readOnly="false" minOccurs="0" maxOccurs="unbounded"/>
    </ECEntityClass>
    <ECEntityClass typeName="TH3" modifier="None">
        <BaseClass>TH2</BaseClass>
        <ECProperty propertyName="S" typeName="string" readOnly="false"/>
        <ECProperty propertyName="S3" typeName="string" readOnly="false"/>
        <ECProperty propertyName="S3_ToBeDeletedInUpgrade" typeName="string" readOnly="false"/>
        <ECArrayProperty propertyName="SA_3_ToBeDeletedInUpgrade" typeName="string" readOnly="false" minOccurs="0" maxOccurs="unbounded"/>
        <ECStructProperty propertyName="SS3_ToBeDeletedInUpgrade" typeName="SC" readOnly="false"/>
        <ECStructArrayProperty propertyName="SSA3_ToBeDeletedInUpgrade" typeName="SC" readOnly="false" minOccurs="0" maxOccurs="unbounded"/>
    </ECEntityClass>
    <ECEntityClass typeName="TestClass" modifier="None"/>
</ECSchema>
