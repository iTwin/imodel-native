        <ECArrayProperty propertyName="SA_1_ToBeDeletedInUpgrade" typeName="string" minOccurs="0" maxOccurs="unbounded" />
        <ECStructProperty propertyName="SS1_ToBeDeletedInUpgrade" typeName="SC" />
        <ECArrayProperty propertyName="SSA1_ToBeDeletedInUpgrade" typeName="SC" minOccurs="0" maxOccurs="unbounded" isStruct="True" />
    </ECClass>
