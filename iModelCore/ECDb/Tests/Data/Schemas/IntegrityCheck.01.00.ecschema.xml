        </ECCustomAttributes>
        <ECProperty propertyName="S" typeName="string" readOnly="false"/>
        <ECProperty propertyName="I" typeName="int" readOnly="false"/>
    </ECEntityClass>
    <ECEntityClass typeName="SourceDerived1" modifier="None">
