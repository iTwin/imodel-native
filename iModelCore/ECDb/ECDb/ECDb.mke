%if defined (BENTLEY_TOOL_CONTEXT_NAME)
%if $(BENTLEY_TOOL_CONTEXT_NAME) == "LinuxGcc"
    LinuxGccCommonOptions = $(OldLinuxGccCommonOptions)
%endif
%endif

%if $(BUILD_TOOLSET) == "CLANG"
    LLVMCommonCompOpts = $(OldLLVMCommonCompOpts)
%endif

