AppName = GridElements
CCompPDBName = $(AppName)

%include mdl.mki
%include $(SrcRoot)Building/ConceptBuilding/privmki/ConceptBuilding.mki

o = $(OutputRootDir)build/ConceptBuilding/GridElements/

dirToSearch = $(GridsPublicApi)
