$(o)GridCurve$(oext) : $(_MakeFilePath)GridCurve.cpp ${MultiCompileDepends} $(_MakeFilePath)PublicApi/GridCurve.h

$(o)GridLine$(oext) : $(_MakeFilePath)GridLine.cpp ${MultiCompileDepends} $(_MakeFilePath)PublicApi/GridLine.h

