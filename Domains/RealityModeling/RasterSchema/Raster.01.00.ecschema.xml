    <ECClass typeName="RasterFileModel" isDomainClass="True" >
        <ECCustomAttributes>
            <ClassHasHandler xmlns="BisCore.01.00" />
        </ECCustomAttributes>
        <BaseClass>RasterModel</BaseClass>
    </ECClass>

