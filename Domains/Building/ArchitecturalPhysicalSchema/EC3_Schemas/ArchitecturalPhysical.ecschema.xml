        <Source multiplicity="(0..1)" polymorphic="true">
            <Class class="Wall"/>
        </Source>
        <Target multiplicity="(0..*)" polymorphic="true">
            <Class class="WallLeaf"/>
        </Target>
    </ECRelationshipClass>
