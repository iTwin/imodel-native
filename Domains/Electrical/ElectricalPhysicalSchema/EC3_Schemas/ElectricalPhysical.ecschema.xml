    <ECEntityClass typeName="Cable" description="This class defines an instance of a cable.">
        <BaseClass>bentley:ElectricalItem</BaseClass>
        <ECProperty propertyName="ESTIMATED_LENGTH" typeName="double" description="Estimated length of the cable." displayLabel="Estimated Length"/>
        <ECProperty propertyName="CABLE_LENGTH" typeName="double" description="Actual length of the cable." displayLabel="Length"/>
        <ECProperty propertyName="SOURCE_LENGTH" typeName="string" description="Added length of cable for termination within the cabinet or equipment." displayLabel="Source Length"/>
        <ECProperty propertyName="TARGET_LENGTH" typeName="string" description="Added length of cable for termination within the cabinet or equipment." displayLabel="Target Length"/>
        <ECProperty propertyName="CURRENT" typeName="string" description="Current passing through the cable." displayLabel="Current"/>
        <ECProperty propertyName="VOLTAGE" typeName="string" description="Voltage applied to the cable." displayLabel="Voltage"/>
        <ECProperty propertyName="NAME" typeName="string" displayLabel="Name">
            <ECCustomAttributes>
                <CalculatedECPropertySpecification xmlns="Bentley_Standard_CustomAttributes.01.13">
                    <ECExpression>this.ID</ECExpression>
                    <FailureValue>&lt;Cable ID&gt;</FailureValue>
                </CalculatedECPropertySpecification>
            </ECCustomAttributes>
        </ECProperty>
    </ECEntityClass>
    <ECEntityClass typeName="Conductor" description="This class defines an instance of a conductor.">
        <BaseClass>bentley:ElectricalItem</BaseClass>
        <ECProperty propertyName="SIZE" typeName="string" description="Size/gauge (outer diameter) of the conductor." displayLabel="Size"/>
        <ECProperty propertyName="COLOR" typeName="string" description="Color of the conductor." displayLabel="Color"/>
        <ECProperty propertyName="Use" typeName="string" description="Specifies the use of the conductor such as 3 phase, control, interlock, etc."/>
        <ECProperty propertyName="TYPE" typeName="string" description="Type of conductor such as THHN, etc." displayLabel="Type"/>
        <ECProperty propertyName="VOLTAGE" typeName="string" description="Voltage applied to the conductor." displayLabel="Voltage"/>
        <ECProperty propertyName="CURRENT" typeName="string" description="Current passing through the conductor." displayLabel="Current"/>
    </ECEntityClass>
