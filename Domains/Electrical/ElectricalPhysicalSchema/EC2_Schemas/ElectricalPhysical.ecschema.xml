<?xml version="1.0" encoding="utf-8"?>
<ECSchema schemaName="Electrical" nameSpacePrefix="elec" version="1.0" description="Bentley Electrical Schema" displayLabel="Bentley Electrical Schema" xmlns="http://www.bentley.com/schemas/Bentley.ECXML.2.0">
    <ECSchemaReference name="Bentley_Standard_CustomAttributes" version="01.01" prefix="bsca" />
    <ECSchemaReference name="BentleyBase" version="01.00" prefix="bentley" />
    <ECCustomAttributes>
        <SchemaChangeTrackingState xmlns="Bentley_Standard_CustomAttributes.01.01">
            <FinalizationHistory />
