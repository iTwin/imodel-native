        </Source>
        <Target cardinality="(0,1)" roleLabel="Cable Is Connected To Target Connection Point (reversed)" polymorphic="True">
            <Class class="ConnectionPoint" />
        </Target>
    </ECRelationshipClass>
    <ECClass typeName="ElectricalItemType" description="This class defines an electrical item type." displayLabel="Electrical Item Type" isDomainClass="True">
        <BaseClass>bentley:BentleyBaseObject</BaseClass>
        <ECProperty propertyName="NAME" typeName="string" description="Name of electrical item type." displayLabel="Name" />
        <ECProperty propertyName="DESCRIPTION_1" typeName="string" description="First technical description of an electrical item type." displayLabel="Description 1" />
        <ECProperty propertyName="DESCRIPTION_2" typeName="string" description="Second technical description of an electrical item type." displayLabel="Description 2" />
        <ECProperty propertyName="DESCRIPTION_3" typeName="string" description="Third technical description of an electrical item type." displayLabel="Description 3" />
    </ECClass>
    <ECClass typeName="CableType" description="Defines a specification for a cable." displayLabel="Cable Type" isDomainClass="True">
        <BaseClass>ElectricalItemType</BaseClass>
        <ECProperty propertyName="SIZE" typeName="string" description="The size/gauge (outer diameter) of the cable." displayLabel="Size" />
        <ECProperty propertyName="SHORT_DESCRIPTION" typeName="string" description="A short description to help identify the cable type." displayLabel="Short Description" />
    </ECClass>
