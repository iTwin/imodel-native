        <ECProperty propertyName="POSITION" typeName="point3d" description="Origin position of a connection point on the drawing." displayLabel="Position" />
    </ECClass>
    <ECRelationshipClass typeName="CableIsConnectedFromSourceConnectionPoint" description="A cable can be connected from a source connection point." displayLabel="Cable Is Connected From Source Connection Point" isDomainClass="True" strength="referencing" strengthDirection="forward">
