        <BaseClass>bentley:BentleyBaseObject</BaseClass>
        <ECProperty propertyName="MANUFACTURER" typeName="string" description="Manufacturer of the electrical item." displayLabel="Manufacturer" />
        <ECProperty propertyName="PART_NUMBER" typeName="string" description="Manufacturer catalog number or internal part number of the electrical item." displayLabel="Part Number" />
        <ECProperty propertyName="HEIGHT" typeName="double" description="Height of the electrical item." displayLabel="Height" />
        <ECProperty propertyName="WIDTH" typeName="double" description="Width of the electrical item." displayLabel="Width" />
        <ECProperty propertyName="DIAMETER" typeName="double" description="Diameter of the electrical item." displayLabel="Diameter" />
        <ECProperty propertyName="DEPTH" typeName="double" description="Depth of the electrical item." displayLabel="Depth" />
        <ECProperty propertyName="WEIGHT" typeName="double" description="Weight of the electrical item." displayLabel="Weight" />
        <ECProperty propertyName="DESCRIPTION_1" typeName="string" description="Technical description of the electrical item." displayLabel="Description 1" />
        <ECProperty propertyName="DESCRIPTION_2" typeName="string" description="Technical description of the electrical item." displayLabel="Description 2" />
        <ECArrayProperty propertyName="PROPERTY_COLLECTION" typeName="PropertyValues" description="A user definable property collection for additional catalog data." displayLabel="Property Collection" minOccurs="0" maxOccurs="1" isStruct="True" />
        <ECProperty propertyName="NAME" typeName="string" displayLabel="Name">
            <ECCustomAttributes>
                <CalculatedECPropertySpecification xmlns="Bentley_Standard_CustomAttributes.01.01">
                    <RequiredSymbolSets />
                    <ECExpression>this.PART_NUMBER</ECExpression>
                    <FailureValue>&lt;Catalog&gt;</FailureValue>
                </CalculatedECPropertySpecification>
            </ECCustomAttributes>
        </ECProperty>
    </ECClass>
