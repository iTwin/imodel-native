        <ECProperty propertyName="POSITION" typeName="point3d" description="Origin position of a device." displayLabel="Position" />
        <ECProperty propertyName="NAME" typeName="string" displayLabel="Name">
            <ECCustomAttributes>
                <CalculatedECPropertySpecification xmlns="Bentley_Standard_CustomAttributes.01.01">
                    <RequiredSymbolSets />
                    <ECExpression>this.ID</ECExpression>
                    <FailureValue>&lt;Device&gt;</FailureValue>
                </CalculatedECPropertySpecification>
            </ECCustomAttributes>
        </ECProperty>
