            <SchemaFullNameAtEditingStart>Electrical.01.00</SchemaFullNameAtEditingStart>
        </SchemaChangeTrackingState>
    </ECCustomAttributes>
    <ECClass typeName="PropertyValues" description="Defines a property/value pair used for property collections." displayLabel="Property Values" isStruct="True" isDomainClass="True">
        <ECProperty propertyName="PROPERTY_NAME" typeName="string" description="Name of a property." displayLabel="Property Name" />
        <ECProperty propertyName="PROPERTY_VALUE" typeName="string" description="Value of a property." displayLabel="Property Value" />
    </ECClass>
    <ECClass typeName="ElectricalItem" description="This class defines instances of electrical items such as devices, cables, raceway, etc." displayLabel="Electrical Item" isDomainClass="True">
        <BaseClass>bentley:BentleyBaseObject</BaseClass>
        <ECProperty propertyName="ID" typeName="string" description="Business ID for an electrical item." />
        <ECProperty propertyName="DESCRIPTION" typeName="string" description="Description of the electrical item." displayLabel="Description" />
        <ECArrayProperty propertyName="PROPERTY_COLLECTION" typeName="PropertyValues" description="A user definable property collection used to define business attribute name and value pairs." displayLabel="Property Collection" minOccurs="0" maxOccurs="1" isStruct="True" />
    </ECClass>
    <ECClass typeName="Cable" description="This class defines an instance of a cable." isDomainClass="True">
        <BaseClass>ElectricalItem</BaseClass>
        <ECProperty propertyName="ESTIMATED_LENGTH" typeName="double" description="Estimated length of the cable." displayLabel="Estimated Length" />
        <ECProperty propertyName="CABLE_LENGTH" typeName="double" description="Actual length of the cable." displayLabel="Length" />
        <ECProperty propertyName="SOURCE_LENGTH" typeName="string" description="Added length of cable for termination within the cabinet or equipment." displayLabel="Source Length" />
        <ECProperty propertyName="TARGET_LENGTH" typeName="string" description="Added length of cable for termination within the cabinet or equipment." displayLabel="Target Length" />
        <ECProperty propertyName="CURRENT" typeName="string" description="Current passing through the cable." displayLabel="Current" />
        <ECProperty propertyName="VOLTAGE" typeName="string" description="Voltage applied to the cable." displayLabel="Voltage" />
        <ECProperty propertyName="NAME" typeName="string" displayLabel="Name">
            <ECCustomAttributes>
                <CalculatedECPropertySpecification xmlns="Bentley_Standard_CustomAttributes.01.01">
                    <RequiredSymbolSets />
                    <ECExpression>this.ID</ECExpression>
                    <FailureValue>&lt;Cable ID&gt;</FailureValue>
                </CalculatedECPropertySpecification>
            </ECCustomAttributes>
        </ECProperty>
    </ECClass>
    <ECClass typeName="Conductor" description="This class defines an instance of a conductor." isDomainClass="True">
        <BaseClass>ElectricalItem</BaseClass>
        <ECProperty propertyName="SIZE" typeName="string" description="Size/gauge (outer diameter) of the conductor." displayLabel="Size" />
        <ECProperty propertyName="COLOR" typeName="string" description="Color of the conductor." displayLabel="Color" />
        <ECProperty propertyName="Use" typeName="string" description="Specifies the use of the conductor such as 3 phase, control, interlock, etc." />
        <ECProperty propertyName="TYPE" typeName="string" description="Type of conductor such as THHN, etc." displayLabel="Type" />
        <ECProperty propertyName="VOLTAGE" typeName="string" description="Voltage applied to the conductor." displayLabel="Voltage" />
        <ECProperty propertyName="CURRENT" typeName="string" description="Current passing through the conductor." displayLabel="Current" />
    </ECClass>
    <ECClass typeName="CableCore" description="This class defines an instance of a cable core." displayLabel="Cable Core" isDomainClass="True">
        <BaseClass>Conductor</BaseClass>
        <ECProperty propertyName="NAME" typeName="string" description="Name of the cable core which could be a color or conductor number." displayLabel="Name" />
    </ECClass>
    <ECRelationshipClass typeName="CableHasCable" description="A cable can be comprised of multiple cables." displayLabel="Cable Has Cable" isDomainClass="True" strength="referencing" strengthDirection="forward">
        <Source cardinality="(0,1)" polymorphic="True">
            <Class class="Cable" />
        </Source>
        <Target cardinality="(0,1)" polymorphic="True">
            <Class class="Cable" />
        </Target>
    </ECRelationshipClass>
    <ECRelationshipClass typeName="CableHasCableCore" description="A cable has one or more cable cores." displayLabel="Cable Has Cable Core" isDomainClass="True" strength="referencing" strengthDirection="forward">
