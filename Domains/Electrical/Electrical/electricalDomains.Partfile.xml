            <Directory ProductDirectoryName="Gtest-Assets" SourceName="Delivery/Gtest/ElectricalTests/Assets" />
        </Bindings>
    </Part>

    <Product Name="Electrical-Gtest">
        <SubPart PartName="Gtest"/>
        <Directories DirectoryListName="GtestProduct" Repository="BeGTest" PartFile="BeGTest"/>
    </Product>

    <Part Name="RunGtest" DeferType="RunUnitTests" BentleyBuildMakeFile="${SrcRoot}BeGTest/gtest/RunGTest.mke" BentleyBuildMakeOptions="-dGTEST_EXE=$(OutputRootDir)Product/Electrical-Gtest/ElectricalTests" OnlyPlatforms="x86,x64,MacOs*,Linux*">
