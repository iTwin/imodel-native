<?xml version="1.0" encoding="UTF-8"?>
<ECSchema schemaName="RoadRailPhysical" alias="rrphys" version="01.00.00" description="Base schema for the Road and Rail domains." xmlns="http://www.bentley.com/schemas/Bentley.ECXML.3.1">
    <ECSchemaReference name="BisCore" version="01.00.00" alias="bis"/>
    <ECSchemaReference name="LinearReferencing" version="01.00.00" alias="lr"/>
    <ECSchemaReference name="RoadRailAlignment" version="01.00.00" alias="rralign"/>
    
    <KindOfQuantity typeName="CURRENCY" displayLabel="CURRENCY" persistenceUnit="ONE" relativeError="1e-2" />
    <KindOfQuantity typeName="DIAMETER" displayLabel="DIAMETER" persistenceUnit="M" relativeError="1e-2" />    
    <KindOfQuantity typeName="LENGTH" displayLabel="LENGTH" persistenceUnit="M" relativeError="1e-2" />    
    <KindOfQuantity typeName="LENGTH_SHORT" displayLabel="LENGTH_SHORT" persistenceUnit="M" relativeError="1e-2" />
    <KindOfQuantity typeName="PLANE_ANGLE" displayLabel="PLANE_ANGLE" persistenceUnit="RAD" relativeError="1e-2" />
    <KindOfQuantity typeName="PERCENT" displayLabel="PERCENT" persistenceUnit="PERCENT" relativeError="1e-2" />

    <ECEntityClass typeName="RoadRailCategoryModel">
        <BaseClass>bis:DefinitionModel</BaseClass>
        <ECCustomAttributes>
            <ClassHasHandler xmlns="BisCore.01.00.00"/>
        </ECCustomAttributes>
    </ECEntityClass>

    <!-- BEGIN CustomAttributes and Enums -->    
    <ECCustomAttributeClass typeName="PropertyPaneHint" appliesTo="Any">
        <ECProperty propertyName="UserDefinedOrDerived" typeName="boolean"/>
    </ECCustomAttributeClass>
    <ECEnumeration typeName="PhysicalElement_Status" backingTypeName="int" isStrict="true">
        <ECEnumerator value="0" displayLabel="Existing"/>
        <ECEnumerator value="1" displayLabel="Proposed"/>
        <ECEnumerator value="2" displayLabel="Construction"/>
    </ECEnumeration>
    <!-- END CustomAttributes and Enums -->

    <!-- BEGIN Boundary element classes -->
    <ECEntityClass typeName="BoundaryElement" modifier="Abstract">
        <BaseClass>bis:SpatialLocationElement</BaseClass>
    </ECEntityClass>
    <ECEntityClass typeName="ConstraintBoundary" modifier="Sealed" displayLabel="Constraint Zone">
        <BaseClass>BoundaryElement</BaseClass>
    </ECEntityClass>
    <ECEntityClass typeName="ParcelBoundary" modifier="Sealed" displayLabel="Parcel">
        <BaseClass>BoundaryElement</BaseClass>
        <ECProperty propertyName="OwnerName" typeName="string" displayLabel="Owner Name" />
        <ECProperty propertyName="Address" typeName="string" displayLabel="Address" />
        <ECProperty propertyName="Descr" typeName="string" displayLabel="Description" />
        <ECProperty propertyName="AssessedValue" typeName="double" displayLabel="Assessed Value" kindOfQuantity="CURRENCY" />
    </ECEntityClass>
    <!-- END Boundary element classes -->

    <!-- BEGIN PathwayElement Class Hierarchy -->
    <ECEntityClass typeName="PathwayElement" modifier="Abstract" description="A long, narrow physical stretch specially constructed for a particular travel type." >
        <BaseClass>bis:PhysicalElement</BaseClass>
        <BaseClass>lr:ILinearElementSource</BaseClass>
        <ECCustomAttributes>
            <ClassHasHandler xmlns="BisCore.01.00.00"/>
        </ECCustomAttributes>
        <ECNavigationProperty propertyName="MainAlignment" relationshipName="PathwayRefersToMainAlignment" direction="Forward"/>
    </ECEntityClass>
    <ECRelationshipClass typeName="PathwayRefersToMainAlignment" strength="referencing" strengthDirection="Forward" modifier="Sealed">
        <Source multiplicity="(0..*)" polymorphic="true" roleLabel="may be refered to by N Pathways">
            <Class class="PathwayElement"/>
        </Source>
        <Target multiplicity="(0..1)" polymorphic="false" roleLabel="refer to 1 Alignment">
            <Class class="rralign:Alignment"/>
        </Target>
    </ECRelationshipClass>
    <ECEntityClass typeName="Roadway" modifier="Sealed" description="A Pathway with a smoothed or paved surface, made for traveling by motor vehicle, carriage, etc." >
        <BaseClass>PathwayElement</BaseClass>
        <ECCustomAttributes>
            <ClassHasHandler xmlns="BisCore.01.00.00"/>
        </ECCustomAttributes>
    </ECEntityClass>
    <ECEntityClass typeName="Railway" modifier="Sealed" description="A Pathway with a track or set of tracks along which passenger and freight trains run." >
        <BaseClass>PathwayElement</BaseClass>
        <ECCustomAttributes>
            <ClassHasHandler xmlns="BisCore.01.00.00"/>
        </ECCustomAttributes>
    </ECEntityClass>
    <!-- END PathwayElement Class Hierarchy -->
    
    <!-- BEGIN TravelwaySegment Element Class Hierarchy -->
    <ECEntityClass typeName="TravelwaySegmentElement" modifier="Abstract" description="Base class for segments of a Travelway, which is the section of the Pathway in direct contact with the moving objects (e.g. motor vehicles, trains, etc)." >
        <BaseClass>bis:PhysicalElement</BaseClass>
        <BaseClass>lr:ILinearlyLocatedElement</BaseClass>
        <ECCustomAttributes>
            <ClassHasHandler xmlns="BisCore.01.00.00"/>
            <ILinearlyLocatedSegmentationHints xmlns="LinearReferencing.01.00.00">
                <SupportedLinearlyReferencedLocationTypes>2</SupportedLinearlyReferencedLocationTypes>
            </ILinearlyLocatedSegmentationHints>
        </ECCustomAttributes>
        <ECProperty propertyName="TravelwayIndex" typeName="int" />
    </ECEntityClass>
    <ECEntityClass typeName="RegularTravelwaySegment" modifier="Sealed" description="Physical segment of a Travelway, with a constant definition." >
        <BaseClass>TravelwaySegmentElement</BaseClass>
        <ECCustomAttributes>
            <ClassHasHandler xmlns="BisCore.01.00.00"/>
        </ECCustomAttributes>
        <ECNavigationProperty propertyName="TravelwayDefinition" relationshipName="RegularSegmentRefersToTravelwayDefinition" direction="Forward"/>
        <ECProperty propertyName="TravelwayDropInterval" typeName="double" kindOfQuantity="lr:LENGTH" />
    </ECEntityClass>
    <ECRelationshipClass typeName="RegularSegmentRefersToTravelwayDefinition" strength="referencing" strengthDirection="Backward" modifier="Sealed">
        <Source multiplicity="(0..*)" polymorphic="false" roleLabel="is referred to by N segments">
            <Class class="RegularTravelwaySegment"/>
        </Source>
        <Target multiplicity="(0..1)" polymorphic="true" roleLabel="refers to 1 definition">
            <Class class="TravelwayDefinitionElement"/>
        </Target>
    </ECRelationshipClass>
    <ECEntityClass typeName="TravelwayTransition" modifier="Sealed" description="Physical segment of a Travelway, with a variable Travelway definition.">
        <BaseClass>TravelwaySegmentElement</BaseClass>
        <ECCustomAttributes>
            <ClassHasHandler xmlns="BisCore.01.00.00"/>
        </ECCustomAttributes>
    </ECEntityClass>
    <ECEntityClass typeName="PathFoundationElement" modifier="Abstract" description="Base class for Road/Rail foundation elements." >
        <BaseClass>bis:PhysicalElement</BaseClass>
        <BaseClass>lr:ILinearlyLocatedElement</BaseClass>
        <ECCustomAttributes>
            <ClassHasHandler xmlns="BisCore.01.00.00"/>
            <ILinearlyLocatedSegmentationHints xmlns="LinearReferencing.01.00.00">
                <SupportedLinearlyReferencedLocationTypes>2</SupportedLinearlyReferencedLocationTypes>
            </ILinearlyLocatedSegmentationHints>
        </ECCustomAttributes>
    </ECEntityClass>
    <ECRelationshipClass typeName="PathwayAssemblesElements" strength="embedding" modifier="Sealed">
        <BaseClass>bis:PhysicalElementAssemblesElements</BaseClass>
        <Source multiplicity="(0..1)" polymorphic="true" roleLabel="is assembled by 1 pathway">
            <Class class="PathwayElement"/>
        </Source>
        <Target multiplicity="(0..*)" polymorphic="true" roleLabel="assembles N elements">
            <Class class="bis:PhysicalElement"/>
        </Target>
    </ECRelationshipClass>
    <!-- END TravelwaySegmentElement Class Hierarchy -->

    <!-- BEGIN RoadIntersectionLeg Element Class Hierarchy -->
    <ECEntityClass typeName="TravelwayIntersectionSegmentElement" modifier="Abstract" description="Physical segment of a Travelway connected to an intersection between alignments." >
        <BaseClass>TravelwaySegmentElement</BaseClass>
        <ECCustomAttributes>
            <ClassHasHandler xmlns="BisCore.01.00.00"/>
        </ECCustomAttributes>
        <ECNavigationProperty propertyName="Intersection" relationshipName="IntersectionSegmentRefersToIntersection" direction="Forward"/>
    </ECEntityClass>
    <ECRelationshipClass typeName="IntersectionSegmentRefersToIntersection" strength="referencing" strengthDirection="Backward" modifier="Sealed">
        <Source multiplicity="(0..*)" polymorphic="true" roleLabel="refers to 1 intersection">
            <Class class="TravelwayIntersectionSegmentElement"/>
        </Source>
        <Target multiplicity="(0..1)" polymorphic="true" roleLabel="is referred by zero or more intersection legs">
            <Class class="AlignmentIntersectionElement"/>
        </Target>
    </ECRelationshipClass>
    <ECEntityClass typeName="RoadIntersectionLegElement" modifier="Abstract" description="Physical segment of the Travelway of a road connected to an intersection." >
        <BaseClass>TravelwayIntersectionSegmentElement</BaseClass>
        <ECCustomAttributes>
            <ClassHasHandler xmlns="BisCore.01.00.00"/>
        </ECCustomAttributes>
        <ECNavigationProperty propertyName="IntersectionLegDefinition" relationshipName="RoadIntersectionLegRefersToDefinition" direction="Forward"/>
    </ECEntityClass>    
    <ECRelationshipClass typeName="RoadIntersectionLegRefersToDefinition" strength="referencing" strengthDirection="Backward" modifier="Sealed">
        <Source multiplicity="(0..*)" polymorphic="false" roleLabel="is referred to by N segments">
            <Class class="RoadIntersectionLegElement"/>
        </Source>
        <Target multiplicity="(0..1)" polymorphic="false" roleLabel="refers to 1 travelway definition">
            <Class class="TravelwayDefinitionElement"/>
        </Target>
    </ECRelationshipClass>
    <!-- END RoadSegmentElement Class Hierarchy -->

    <!-- BEGIN PathFoundationSegment Class Hierarchy -->
    <ECEnumeration typeName="TravelwaySide" backingTypeName="int" isStrict="true">
        <ECEnumerator value="0" displayLabel="Left"/>
        <ECEnumerator value="1" displayLabel="Right"/>
    </ECEnumeration>
    <ECEntityClass typeName="PathFoundationSegment" modifier="None" description="Generic class for Road/Rail foundation segment elements." >
        <BaseClass>PathFoundationElement</BaseClass>
    </ECEntityClass>
    <ECEntityClass typeName="PathFoundationIntersectionSegment" modifier="None" description="Generic class for Road/Rail foundation intersection elements." >
        <BaseClass>PathFoundationElement</BaseClass>
    </ECEntityClass>    
    <ECEntityClass typeName="EndConditionSegment" modifier="None" description="Generic class for Road/Rail end-condition elements." >
        <BaseClass>PathFoundationElement</BaseClass>
        <ECNavigationProperty propertyName="EndConditionDefinition" relationshipName="EndConditionSegmentRefersToDefinition" direction="Forward"/>
        <ECProperty propertyName="TravelwaySide" typeName="TravelwaySide"/>
        <ECProperty propertyName="TravelwayIndex" typeName="int"/>
    </ECEntityClass>
    <ECRelationshipClass typeName="EndConditionSegmentRefersToDefinition" strength="referencing" strengthDirection="Backward" modifier="Sealed">
        <Source multiplicity="(0..*)" polymorphic="false" roleLabel="is referred to by N segments">
            <Class class="EndConditionSegment"/>
        </Source>
        <Target multiplicity="(0..1)" polymorphic="false" roleLabel="refers to 1 definition">
            <Class class="EndConditionDefinition"/>
        </Target>
    </ECRelationshipClass>
    <ECEntityClass typeName="BufferSegment" modifier="None" description="Generic class for Road/Rail buffer elements." >
        <BaseClass>PathFoundationElement</BaseClass>
        <ECNavigationProperty propertyName="BufferDefinition" relationshipName="BufferSegmentRefersToDefinition" direction="Forward"/>
        <ECProperty propertyName="TravelwaySide" typeName="TravelwaySide"/>
        <ECProperty propertyName="TravelwayIndex" typeName="int"/>
    </ECEntityClass>
    <ECRelationshipClass typeName="BufferSegmentRefersToDefinition" strength="referencing" strengthDirection="Backward" modifier="Sealed">
        <Source multiplicity="(0..*)" polymorphic="false" roleLabel="is referred to by N segments">
            <Class class="BufferSegment"/>
        </Source>
        <Target multiplicity="(0..1)" polymorphic="false" roleLabel="refers to 1 definition">
            <Class class="BufferDefinition"/>
        </Target>
    </ECRelationshipClass>
    <!-- END PathFoundationSegment Class Hierarchy -->

    <!-- BEGIN AlignmentIntersectionElement Class Hierarchy -->
    <ECEntityClass typeName="AlignmentIntersectionElement" modifier="Abstract" description="Base class for Physical intersections driven by alignments.">
        <BaseClass>bis:PhysicalElement</BaseClass>
    </ECEntityClass>
    <ECEntityClass typeName="RoadIntersectionElement" modifier="Abstract" description="Base class for Physical road intersections.">
        <BaseClass>AlignmentIntersectionElement</BaseClass>
    </ECEntityClass>
    <ECEntityClass typeName="RoadCrossing" modifier="Sealed" description="Physical road crossing-intersection.">
        <BaseClass>RoadIntersectionElement</BaseClass>
    </ECEntityClass>
    <ECEntityClass typeName="BranchIntersection" modifier="Sealed" description="Physical road branching-intersection.">
        <BaseClass>RoadIntersectionElement</BaseClass>
    </ECEntityClass>
    <ECEntityClass typeName="Roundabout" modifier="Sealed" description="Physical road roundabout.">
        <BaseClass>RoadIntersectionElement</BaseClass>
    </ECEntityClass>
    <!-- END AlignmentIntersectionElement Class Hierarchy -->

    <!-- BEGIN Tunnel-related Classes -->
    <ECEntityClass typeName="Tunnel" modifier="Sealed" description="Physical tunnel which a segment of a road or rail go through." >
        <BaseClass>bis:PhysicalElement</BaseClass>
        <BaseClass>lr:ILinearElement</BaseClass>
    </ECEntityClass>
    <!-- END Tunnel-related Classes -->
    
    <!-- BEGIN Linearly-located PhysicalElement Class Hierarchy (Furniture-related) -->
    <ECEntityClass typeName="GenericLinearlyLocatedPhysicalElement" modifier="Sealed" description="Generic Physical Element that is linearly located.">
        <BaseClass>bis:PhysicalElement</BaseClass>
        <BaseClass>lr:ILinearlyLocatedElement</BaseClass>
    </ECEntityClass>
    <ECEntityClass typeName="LinearlyLocatedPhysicalElementArray" modifier="Abstract" description="Base class for a linearly-located array of physical elements.">
        <BaseClass>bis:PhysicalElement</BaseClass>
        <BaseClass>lr:ILinearlyLocatedElement</BaseClass>
        <ECProperty propertyName="ForceLastGeometry" typeName="boolean"/>
        <ECProperty propertyName="Spacing" typeName="double" kindOfQuantity="lr:LENGTH"/>
    </ECEntityClass>
    <ECEntityClass typeName="GenericLinearlyLocatedPhysicalElementArray" modifier="Sealed" description="Generic linearly-located array of physical element.">
        <BaseClass>LinearlyLocatedPhysicalElementArray</BaseClass>
    </ECEntityClass>
    <!-- END Linearly-located PhysicalElement Class Hierarchy -->

    <!-- BEGIN PhysicalType Class Hierarchy (Furniture-related) -->
    <ECEntityClass typeName="GenericFurnitureType" modifier="Sealed" description="Physical type for generic furniture elements. Template graphics expected to be stored in a related bis:TemplateRecipe3d.">
        <BaseClass>bis:PhysicalType</BaseClass>
        <ECProperty propertyName="Thumbnail" typeName="binary" />
        <ECProperty propertyName="LightsJson" typeName="string" />
    </ECEntityClass>
    <ECEntityClass typeName="GuardRailType" modifier="Sealed" description="Physical type identifying Guard-Rail. Graphics may optionally be stored in a related TemplateRecipe3d.">
        <BaseClass>bis:PhysicalType</BaseClass>
    </ECEntityClass>
    <!-- END PhysicalType Class Hierarchy -->

    <!-- BEGIN TemplateRecipe3d Class Hierarchy -->
    <ECEntityClass typeName="GenericRepetitiveTemplateRecipe3d" modifier="Sealed" description="TemplateRecipe that generates graphics by repeating a GeometryPart, in between a start and end parts.">
        <BaseClass>bis:TemplateRecipe3d</BaseClass>
        <ECProperty propertyName="DefaultSpacing" typeName="double" kindOfQuantity="LENGTH"/>
        <ECNavigationProperty propertyName="StartGeometryPart" relationshipName="PartEncapsulatedAsStartGeometry" direction="Backward" />
        <ECNavigationProperty propertyName="EndGeometryPart" relationshipName="PartEncapsulatedAsEndGeometry" direction="Backward" />
        <ECNavigationProperty propertyName="RepeatedGeometryPart" relationshipName="PartEncapsulatedAsRepeatedGeometry" direction="Backward" />
        <ECNavigationProperty propertyName="ExtrudedGeometryPart" relationshipName="PartEncapsulatedAsExtrudedGeometry" direction="Backward" />
    </ECEntityClass>
    <ECRelationshipClass typeName="PartEncapsulatedAsStartGeometry" strength="referencing" modifier="Sealed">
        <Source multiplicity="(0..1)" polymorphic="false" roleLabel="encapsulates 1 part">
            <Class class="bis:GeometryPart"/>
        </Source>
        <Target multiplicity="(0..1)" polymorphic="true" roleLabel="encapsulated by 1 definition">
            <Class class="GenericRepetitiveTemplateRecipe3d"/>
        </Target>
    </ECRelationshipClass>
    <ECRelationshipClass typeName="PartEncapsulatedAsEndGeometry" strength="referencing" modifier="Sealed">
        <Source multiplicity="(0..1)" polymorphic="false" roleLabel="encapsulates 1 part">
            <Class class="bis:GeometryPart"/>
        </Source>
        <Target multiplicity="(0..1)" polymorphic="true" roleLabel="encapsulated by 1 definition">
            <Class class="GenericRepetitiveTemplateRecipe3d"/>
        </Target>
    </ECRelationshipClass>
    <ECRelationshipClass typeName="PartEncapsulatedAsRepeatedGeometry" strength="referencing" modifier="Sealed">
        <Source multiplicity="(0..1)" polymorphic="false" roleLabel="encapsulates 1 part">
            <Class class="bis:GeometryPart"/>
        </Source>
        <Target multiplicity="(0..1)" polymorphic="true" roleLabel="encapsulated by 1 definition">
            <Class class="GenericRepetitiveTemplateRecipe3d"/>
        </Target>
    </ECRelationshipClass>
    <ECRelationshipClass typeName="PartEncapsulatedAsExtrudedGeometry" strength="referencing" modifier="Sealed">
        <Source multiplicity="(0..1)" polymorphic="false" roleLabel="encapsulates 1 part">
            <Class class="bis:GeometryPart"/>
        </Source>
        <Target multiplicity="(0..1)" polymorphic="true" roleLabel="encapsulated by 1 definition">
            <Class class="GenericRepetitiveTemplateRecipe3d"/>
        </Target>
    </ECRelationshipClass>
    <ECEntityClass typeName="LinearTemplateRecipe3d" modifier="Sealed" description="TemplateRecipe that generates graphics by dropping a GeometryPart along a linear-path.">
        <BaseClass>bis:TemplateRecipe3d</BaseClass>
        <ECProperty propertyName="DropInterval" typeName="double" kindOfQuantity="LENGTH" />
    </ECEntityClass>
    <ECEntityClass typeName="SurfaceTemplateRecipe3d" modifier="Sealed" description="TemplateRecipe that generates graphics by creating a volume from a GeometryPart applied on an Area.">
        <BaseClass>bis:TemplateRecipe3d</BaseClass>
    </ECEntityClass>
    <!-- END TemplateRecipe3d Class Hierarchy -->
    
    <!-- BEGIN General Aspects classes -->
    <ECEntityClass typeName="StatusAspect" modifier="Sealed" description="UniqueAspect to be applied to Physical Elements whose status needs to be tracked.">
        <BaseClass>bis:ElementUniqueAspect</BaseClass>
        <ECCustomAttributes>
            <ClassHasHandler xmlns="BisCore.01.00.00"/>
        </ECCustomAttributes>
        <ECProperty propertyName="Status" typeName="PhysicalElement_Status" displayLabel="Status" />
    </ECEntityClass>
    <ECEntityClass typeName="LinearlyLocatedStatus" modifier="Sealed" description="Attribution element enabling linearly-referenced Status assignment.">
        <BaseClass>bis:InformationRecordElement</BaseClass>
        <BaseClass>lr:ILinearlyLocatedAttribution</BaseClass>
        <ECCustomAttributes>
            <ClassHasHandler xmlns="BisCore.01.00.00"/>
        </ECCustomAttributes>
        <ECProperty propertyName="Status" typeName="PhysicalElement_Status" displayLabel="Status" />
    </ECEntityClass>
    <ECRelationshipClass typeName="PhysicalElementOwnsLinearlyLocatedStatus" strength="embedding" modifier="Sealed">
        <BaseClass>bis:ElementOwnsChildElements</BaseClass>
        <Source multiplicity="(0..1)" polymorphic="true" roleLabel="are about 1 physical element">
            <Class class="bis:PhysicalElement"/>
        </Source>
        <Target multiplicity="(0..*)" polymorphic="false" roleLabel="owns N linearly-located status">
            <Class class="LinearlyLocatedStatus"/>
        </Target>
    </ECRelationshipClass>
    <ECRelationshipClass typeName="ElementOwnsStatusAspect" strength="embedding" modifier="Sealed">
        <BaseClass>bis:ElementOwnsUniqueAspect</BaseClass>
        <Source multiplicity="(1..1)" polymorphic="true" roleLabel="is owned by 1 element">
            <Class class="bis:Element"/>
        </Source>
        <Target multiplicity="(0..1)" polymorphic="false" roleLabel="owns 1 status-aspect">
            <Class class="StatusAspect"/>
        </Target>
    </ECRelationshipClass>
    <ECEntityClass typeName="PhysicalElementDataCache" modifier="Sealed" description="UniqueAspect to be applied to Physical Elements to hold general cached-data.">
        <BaseClass>bis:ElementUniqueAspect</BaseClass>
        <ECProperty propertyName="TerrainClippingMask" typeName="Bentley.Geometry.Common.IGeometry" kindOfQuantity="LENGTH" />
        <ECProperty propertyName="DerivedPropertiesCache" typeName="string" description="Json string containing derived properties, typically computed by an EGA"/>
    </ECEntityClass>
    <ECRelationshipClass typeName="ElementOwnsDataCache" strength="embedding" modifier="Sealed">
        <BaseClass>bis:ElementOwnsUniqueAspect</BaseClass>
        <Source multiplicity="(1..1)" polymorphic="true" roleLabel="is owned by 1 element">
            <Class class="bis:Element"/>
        </Source>
        <Target multiplicity="(0..1)" polymorphic="false" roleLabel="owns 1 data-cache aspect">
            <Class class="PhysicalElementDataCache"/>
        </Target>
    </ECRelationshipClass>
    <!-- END General Aspects classes -->

    <!-- BEGIN Road Design-Speed classes -->
    <ECEntityClass typeName="RoadwayStandardsModel">
        <BaseClass>bis:DefinitionModel</BaseClass>
        <ECCustomAttributes>
            <ClassHasHandler xmlns="BisCore.01.00.00"/>
        </ECCustomAttributes>
    </ECEntityClass>
    <ECEntityClass typeName="RoadDesignSpeedStandards">
        <BaseClass>bis:DefinitionElement</BaseClass>
        <BaseClass>bis:ISubModeledElement</BaseClass>
        <ECCustomAttributes>
            <ClassHasHandler xmlns="BisCore.01.00.00"/>
        </ECCustomAttributes>
    </ECEntityClass>
    <ECEntityClass typeName="RoadDesignSpeedDefinitionTableModel">
        <BaseClass>bis:DefinitionModel</BaseClass>
        <ECCustomAttributes>
            <ClassHasHandler xmlns="BisCore.01.00.00"/>
        </ECCustomAttributes>
    </ECEntityClass>
    <ECEntityClass typeName="RoadDesignSpeedDefinitionTable">
        <BaseClass>bis:DefinitionElement</BaseClass>
        <BaseClass>bis:ISubModeledElement</BaseClass>
        <ECCustomAttributes>
            <ClassHasHandler xmlns="BisCore.01.00.00"/>
        </ECCustomAttributes>
        <ECProperty propertyName="UnitSystem" typeName="int" />
    </ECEntityClass>
    <ECEntityClass typeName="RoadDesignSpeedDefinitionModel">
        <BaseClass>bis:DefinitionModel</BaseClass>
        <ECCustomAttributes>
            <ClassHasHandler xmlns="BisCore.01.00.00"/>
        </ECCustomAttributes>
    </ECEntityClass>
    <ECEntityClass typeName="RoadDesignSpeedDefinition">
        <BaseClass>bis:DefinitionElement</BaseClass>
        <ECCustomAttributes>
            <ClassHasHandler xmlns="BisCore.01.00.00"/>
        </ECCustomAttributes>
        <ECProperty propertyName="SideSlopeFrictionFactor" typeName="double" />
        <ECProperty propertyName="DesignSpeed" typeName="double" />
        <ECProperty propertyName="MaxTransitionRadius" typeName="double" />
        <ECProperty propertyName="DesirableSpiralLength" typeName="double" />
        <ECProperty propertyName="Type1CrestVertical" typeName="double" />
        <ECProperty propertyName="Type3SagVertical" typeName="double" />
        <ECProperty propertyName="CrestVerticalPsd" typeName="double" />
        <ECProperty propertyName="AverageRunningSpeed" typeName="double" />
        <ECProperty propertyName="MaximumRelativeGradient" typeName="double" />
        <ECProperty propertyName="CrestK_SSD" typeName="double" />
        <ECProperty propertyName="SagK_SSD" typeName="double" />
        <ECProperty propertyName="IgnoreLength" typeName="double" />        
    </ECEntityClass>
    <ECRelationshipClass typeName="RoadDesignSpeedRefersToDefinition" strength="referencing" strengthDirection="Backward" modifier="Sealed">
        <Source multiplicity="(0..*)" polymorphic="false" roleLabel="is referred to by N design-speeds">
            <Class class="RoadDesignSpeed"/>
        </Source>
        <Target multiplicity="(0..1)" polymorphic="false" roleLabel="refers to 1 definition">
            <Class class="RoadDesignSpeedDefinition"/>
        </Target>
    </ECRelationshipClass>
    <ECEntityClass typeName="RoadDesignSpeed" modifier="Sealed"  displayLabel="Design Speed" description="Linearly located property of a Roadway specifying design-speeds along it." >
        <BaseClass>bis:InformationRecordElement</BaseClass>
        <BaseClass>lr:ILinearlyLocatedAttribution</BaseClass>
        <ECCustomAttributes>
            <ClassHasHandler xmlns="BisCore.01.00.00"/>
            <ILinearlyLocatedSegmentationHints xmlns="LinearReferencing.01.00.00">
                <SupportedLinearlyReferencedLocationTypes>2</SupportedLinearlyReferencedLocationTypes>
            </ILinearlyLocatedSegmentationHints>
        </ECCustomAttributes>
        <ECNavigationProperty propertyName="RoadDesignSpeedDefinition" relationshipName="RoadDesignSpeedRefersToDefinition" direction="Forward"/>
    </ECEntityClass>
    <ECRelationshipClass typeName="RoadwayOwnsDesignSpeeds" strength="embedding" modifier="Sealed">
        <BaseClass>bis:ElementOwnsChildElements</BaseClass>
        <Source multiplicity="(0..1)" polymorphic="false" roleLabel="is owned by 1 Roadway">
            <Class class="Roadway"/>
        </Source>
        <Target multiplicity="(0..*)" polymorphic="false" roleLabel="owns N design speeds">
            <Class class="RoadDesignSpeed"/>
        </Target>
    </ECRelationshipClass>
    <!-- END Road Design-Speed classes -->

    <!-- BEGIN RoadClass classes -->
    <ECEntityClass typeName="RoadClassStandards">
        <BaseClass>bis:DefinitionElement</BaseClass>
        <BaseClass>bis:ISubModeledElement</BaseClass>
        <ECCustomAttributes>
            <ClassHasHandler xmlns="BisCore.01.00.00"/>
        </ECCustomAttributes>
    </ECEntityClass>
    <ECEntityClass typeName="RoadClassDefinitionTableModel">
        <BaseClass>bis:DefinitionModel</BaseClass>
        <ECCustomAttributes>
            <ClassHasHandler xmlns="BisCore.01.00.00"/>
        </ECCustomAttributes>
    </ECEntityClass>
    <ECEntityClass typeName="RoadClassDefinitionTable">
        <BaseClass>bis:DefinitionElement</BaseClass>
        <BaseClass>bis:ISubModeledElement</BaseClass>
        <ECCustomAttributes>
            <ClassHasHandler xmlns="BisCore.01.00.00"/>
        </ECCustomAttributes>
    </ECEntityClass>
    <ECEntityClass typeName="RoadClassDefinitionModel">
        <BaseClass>bis:DefinitionModel</BaseClass>
        <ECCustomAttributes>
            <ClassHasHandler xmlns="BisCore.01.00.00"/>
        </ECCustomAttributes>
    </ECEntityClass>
    <ECEntityClass typeName="RoadClassDefinition">
        <BaseClass>bis:DefinitionElement</BaseClass>
        <ECCustomAttributes>
            <ClassHasHandler xmlns="BisCore.01.00.00"/>
        </ECCustomAttributes>
    </ECEntityClass>
