<?xml version="1.0" encoding="UTF-8"?>
<ECSchema schemaName="Profiles" alias="CP" description="EC Schema for Profiles" version="02.00.00" displayLabel="Profiles" xmlns="http://www.bentley.com/schemas/Bentley.ECXML.3.1">
    <ECSchemaReference name="BisCore" version="01.00.00" alias="bis"/>
    <ECEntityClass typeName="ICustomProfile" displayLabel="Custom Profile Mixin">
        <ECCustomAttributes>
            <IsMixin xmlns="CoreCustomAttributes.1.0">
                <AppliesToEntityClass>bis:DefinitionElement</AppliesToEntityClass>
            </IsMixin>
        </ECCustomAttributes>
    </ECEntityClass>
    <ECEntityClass typeName="ICircularProfile" displayLabel="Circular Profile Mixin">
        <ECCustomAttributes>
            <IsMixin xmlns="CoreCustomAttributes.1.0">
                <AppliesToEntityClass>bis:DefinitionElement</AppliesToEntityClass>
            </IsMixin>
        </ECCustomAttributes>
        <ECProperty propertyName="OuterRadius" typeName="double"/>
    </ECEntityClass>
    <ECEntityClass typeName="ICShapeProfile" displayLabel="C-Shape Profile Mixin">
        <ECCustomAttributes>
            <IsMixin xmlns="CoreCustomAttributes.1.0">
                <AppliesToEntityClass>bis:DefinitionElement</AppliesToEntityClass>
            </IsMixin>
        </ECCustomAttributes>
        <ECProperty propertyName="Width" typeName="double" />
        <ECProperty propertyName="Depth" typeName="double" />
    </ECEntityClass>
    <ECEntityClass typeName="ILShapeProfile" displayLabel="L-Shape Profile Mixin">
        <ECCustomAttributes>
            <IsMixin xmlns="CoreCustomAttributes.1.0">
                <AppliesToEntityClass>bis:DefinitionElement</AppliesToEntityClass>
            </IsMixin>
        </ECCustomAttributes>
        <ECProperty propertyName="Width" typeName="double" />
        <ECProperty propertyName="Depth" typeName="double" />
    </ECEntityClass>
    <ECEntityClass typeName="ICenterLineProfile" displayLabel="Center Line Profile Mixin">
        <ECCustomAttributes>
            <IsMixin xmlns="CoreCustomAttributes.1.0">
                <AppliesToEntityClass>bis:DefinitionElement</AppliesToEntityClass>
            </IsMixin>
        </ECCustomAttributes>
        <!--not sure of the TYPE of this 'CenterLine' property, placeholder for now-->
        <ECProperty propertyName="CenterLine" typeName="Bentley.Geometry.Common.ICurve" />
        <ECProperty propertyName="WallThickness" typeName="double" />
    </ECEntityClass>
    <ECEntityClass typeName="IRectangleShapeProfile" displayLabel="Rectangle-Shape Profile Mixin">
        <ECCustomAttributes>
            <IsMixin xmlns="CoreCustomAttributes.1.0">
                <AppliesToEntityClass>bis:DefinitionElement</AppliesToEntityClass>
            </IsMixin>
        </ECCustomAttributes>
        <ECProperty propertyName="XDim" typeName="double" />
        <ECProperty propertyName="YDim" typeName="double" />
    </ECEntityClass>
    <ECEntityClass typeName="Profile" description="A resource defining one or more 2D areas that may have voids.">
        <BaseClass>bis:DefinitionElement</BaseClass>
        <!--not sure of the TYPE of this 'Shape' property, placeholder for now-->
        <ECProperty propertyName="Shape" typeName="Bentley.Geometry.Common.IGeometry"/>
    </ECEntityClass>
    <ECEntityClass typeName="SinglePerimiterProfile" description="A Profile with a single outer perimiter.">
        <BaseClass>Profile</BaseClass>
    </ECEntityClass>
    <ECEntityClass typeName="CenteredProfile" description="A SinglePerimeterProfile that is guaranteed to have the center of the bounding box at 0,0.">
        <BaseClass>SinglePerimiterProfile</BaseClass>
    </ECEntityClass>
    <ECEntityClass typeName="AsymmetricIShapeProfile" description="An asymmetric (about x-axis) I shaped Profile similar to rolled steel I-shapes.">
        <BaseClass>CenteredProfile</BaseClass>
        <ECProperty propertyName="BottomFlangeWidth" typeName="double"/>
        <ECProperty propertyName="OverallDepth" typeName="double"/>
        <ECProperty propertyName="WebThickness" typeName="double"/>
        <ECProperty propertyName="BottomFlangeThickness" typeName="double"/>
        <ECProperty propertyName="TopFlangeWidth" typeName="double"/>
        <!--Optional Properties-->
        <ECProperty propertyName="BottomFlangeFilletRadius" typeName="double"/>
        <ECProperty propertyName="BottomFlangeSlope" typeName="double"/>
        <ECProperty propertyName="BottomFlangeEdgeRadius" typeName="double"/>
        <ECProperty propertyName="TopFlangeThickness" typeName="double"/>
        <ECProperty propertyName="TopFlangeEdgeRadius" typeName="double"/>
        <ECProperty propertyName="TopFlangeFilletRadius" typeName="double"/>
        <ECProperty propertyName="TopFlangeSlope" typeName="double"/>
    </ECEntityClass>
    <ECEntityClass typeName="BentPlateProfile">
        <BaseClass>CenteredProfile</BaseClass>
        <ECProperty propertyName="CenterLine" typeName="Bentley.Geometry.Common.ICurve"/>
        <ECProperty propertyName="PlateThickness" typeName="double"/>
        <ECProperty propertyName="PlateWidth" typeName="double"/>
        <ECProperty propertyName="BendAngle" typeName="double"/>
        <ECProperty propertyName="BendRadius" typeName="double"/>
    </ECEntityClass>
    <ECEntityClass typeName="CenterLineCShapeProfile" description="A C shaped Profile similar to cold-formed steel C-shapes.">
        <BaseClass>CenteredProfile</BaseClass>
        <BaseClass>ICShapeProfile</BaseClass>
        <BaseClass>ICenterLineProfile</BaseClass>
        <ECProperty propertyName="InsideFilletRadius" typeName="double"/>
        <ECProperty propertyName="Girth" typeName="double" description="Lip Length"/>
    </ECEntityClass>
    <ECEntityClass typeName="CenterLineLShapeProfile" description="An L-shaped Profile similar to cold-formed steel L-shapes.">
        <BaseClass>CenteredProfile</BaseClass>
        <BaseClass>ILShapeProfile</BaseClass>
        <BaseClass>ICenterLineProfile</BaseClass>
        <ECProperty propertyName="InsideFilletRadius" typeName="double"/>
        <ECProperty propertyName="Girth" typeName="double" description="Lip Length"/>
    </ECEntityClass>
    <ECEntityClass typeName="CenterLineZShapeProfile" description="A Z-shaped Profile similar to cold-formed steel Z-shapes">
        <BaseClass>CenteredProfile</BaseClass>
        <BaseClass>ICenterLineProfile</BaseClass>
        <ECProperty propertyName="InsideFilletRadius" typeName="double"/>
        <ECProperty propertyName="Girth" typeName="double" description="Lip Length"/>
        <ECProperty propertyName="Depth" typeName="double"/>
        <ECProperty propertyName="FlangeWidth" typeName="double"/>
    </ECEntityClass>
    <ECEntityClass typeName="CompositeProfile" description="A Profile comprised of multiple SinglePerimiterProfiles.">
        <BaseClass>Profile</BaseClass>
    </ECEntityClass>
    <ECEntityClass typeName="CShapeProfile" description="A C shaped Profile similar to rolled steel C-shapes.">
        <BaseClass>CenteredProfile</BaseClass>
        <ECProperty propertyName="Depth" typeName="double"/>
        <ECProperty propertyName="Width" typeName="double"/>
        <ECProperty propertyName="WallThickness" typeName="double"/>
        <ECProperty propertyName="Girth" typeName="double"/>
        <!--Optional-->
        <ECProperty propertyName="InternalFilletRadius" typeName="double"/>
    </ECEntityClass>
    <ECEntityClass typeName="CustomCenterLineProfile">
        <BaseClass>SinglePerimiterProfile</BaseClass>
        <BaseClass>ICustomProfile</BaseClass>
        <BaseClass>ICenterLineProfile</BaseClass>
    </ECEntityClass>
    <ECEntityClass typeName="CustomCompositeProfile" description="A Profile comprised of multiple SinglePerimeterProfiles.">
        <BaseClass>CompositeProfile</BaseClass>
    </ECEntityClass>
    <ECEntityClass typeName="CustomShapeProfile">
        <BaseClass>SinglePerimiterProfile</BaseClass>
        <BaseClass>ICustomProfile</BaseClass>
        <!--not sure of the TYPE of these curve properties (taken from IFC), placeholder for now-->
        <ECProperty propertyName="OuterCurve" typeName="Bentley.Geometry.Common.ICurve"/>
        <!--Optional-->
        <ECArrayProperty propertyName="InnerCurves" typeName="Bentley.Geometry.Common.ICurve" minOccurs="1" maxOccurs="unbounded"/>
    </ECEntityClass>
    <ECEntityClass typeName="DerivedProfile" description="A SinglePerimeterProfile that has been converted into a different shape via some mathematical operation.">
        <BaseClass>SinglePerimiterProfile</BaseClass>
    </ECEntityClass>
    <ECEntityClass typeName="DeformedProfile" description="Provided for IfcDerivedProfile compatibility where scaling of profiles is used.">
        <BaseClass>DerivedProfile</BaseClass>
        <!--not sure of the TYPE of this 'Transformation' property, placeholder for now-->
        <ECProperty propertyName="Transformation" typeName="Bentley.Geometry.Common.ITransformedGeometry"/>
    </ECEntityClass>
    <ECEntityClass typeName="DoubleCProfile" description="A CompositeProfile comprised of back-to-back Cs.">
        <BaseClass>CompositeProfile</BaseClass>
        <ECProperty propertyName="Spacing" typeName="double"/>
    </ECEntityClass>
    <ECEntityClass typeName="DoubleLProfile" description="A CompositeProfile comprised of back-to-back Ls with the horizontal legs at the top of the Profile.">
        <BaseClass>CompositeProfile</BaseClass>
        <ECProperty propertyName="Spacing" typeName="double"/>
        <ECProperty propertyName="enum" typeName="int" description="(SLBB vs LLBB)"/>
    </ECEntityClass>
    <ECEntityClass typeName="EllipseProfile">
        <BaseClass>CenteredProfile</BaseClass>
        <ECProperty propertyName="SemiAxis2" typeName="double"/>
        <ECProperty propertyName="SemiAxis1" typeName="double"/>
    </ECEntityClass>
    <ECEntityClass typeName="HollowCircleProfile">
        <BaseClass>CenteredProfile</BaseClass>
        <BaseClass>ICenterLineProfile</BaseClass>
        <ECProperty propertyName="Radius" typeName="double"/>
    </ECEntityClass>
    <ECEntityClass typeName="HollowRectangleProfile">
        <BaseClass>CenteredProfile</BaseClass>
        <BaseClass>ICenterLineProfile</BaseClass>
        <BaseClass>IRectangleShapeProfile</BaseClass>
        <!--Optional Properties-->
        <ECProperty propertyName="InnerFilletRadius" typeName="double"/>
        <ECProperty propertyName="OuterFilletRadius" typeName="double"/>
    </ECEntityClass>
    <ECEntityClass typeName="IShapeProfile" description="An I shaped Profile similar to rolled steel I-shapes.">
        <BaseClass>CenteredProfile</BaseClass>
        <ECProperty propertyName="OverallWidth" typeName="double"/>
        <ECProperty propertyName="OverallDepth" typeName="double"/>
        <ECProperty propertyName="WebThickness" typeName="double"/>
        <ECProperty propertyName="FlangeThickness" typeName="double"/>
        <!--Optional Properties-->
        <ECProperty propertyName="FilletRadius" typeName="double"/>
        <ECProperty propertyName="FlangeEdgeRadius" typeName="double"/>
        <ECProperty propertyName="FlangeSlope" typeName="double"/>
    </ECEntityClass>
    <ECEntityClass typeName="LShapeProfile" description="An L-shaped Profile similar to rolled steel L-shapes.">
        <BaseClass>CenteredProfile</BaseClass>
        <BaseClass>ILShapeProfile</BaseClass>
        <ECProperty propertyName="Thickness" typeName="double"/>
        <!--Optional Properties-->
        <ECProperty propertyName="EdgeRadius" typeName="double"/>
        <ECProperty propertyName="FilletRadius" typeName="double"/>
        <ECProperty propertyName="LegSlope" typeName="double"/>
    </ECEntityClass>
    <ECRelationshipClass typeName="Profile_CompositeProfile" modifier="None" strength="referencing">
        <BaseClass>bis:ElementRefersToElements</BaseClass>
        <Source multiplicity="(0..*)" roleLabel="is profile for" polymorphic="true">
            <Class class="SinglePerimiterProfile"/>
        </Source>
        <Target multiplicity="(0..1)" roleLabel="has profiles" polymorphic="true">
            <Class class="CompositeProfile"/>
        </Target>
    </ECRelationshipClass>
    <ECRelationshipClass typeName="Profile_DerivedProfile" modifier="None" strength="embedding">
        <BaseClass>bis:ElementOwnsChildElements</BaseClass>
        <Source multiplicity="(0..1)" roleLabel="is base profile of" polymorphic="true">
            <Class class="Profile"/>
        </Source>
        <Target multiplicity="(0..*)" roleLabel="is derived profile of" polymorphic="true">
            <Class class="DerivedProfile"/>
        </Target>
    </ECRelationshipClass>
    <ECRelationshipClass typeName="LProfile_DoubleLProfile" modifier="None" strength="referencing">
        <BaseClass>bis:ElementRefersToElements</BaseClass>
        <Source multiplicity="(0..2)" roleLabel="is profile for" polymorphic="True">
            <Class class="ILShapeProfile" />
        </Source>
        <Target multiplicity="(0..1)" roleLabel="is double profile of" polymorphic="True">
            <Class class="DoubleLProfile" />
        </Target>
    </ECRelationshipClass>
    <ECRelationshipClass typeName="CProfile_DoubleCProfile" modifier="None" strength="referencing">
        <BaseClass>bis:ElementRefersToElements</BaseClass>
        <Source multiplicity="(0..2)" roleLabel="is profile for" polymorphic="True">
            <Class class="ICShapeProfile" />
        </Source>
        <Target multiplicity="(0..1)" roleLabel="is double profile of" polymorphic="True">
            <Class class="DoubleCProfile" />
        </Target>
    </ECRelationshipClass>
    <ECRelationshipClass typeName="ProfileHasStandardProfileAspect" modifier="None" strength="embedding">
        <BaseClass>bis:ElementOwnsUniqueAspect</BaseClass>
        <Source multiplicity="(1..1)" roleLabel="owns" polymorphic="True">
            <Class class="Profile" />
        </Source>
        <Target multiplicity="(0..*)" roleLabel="is owned by" polymorphic="True">
            <Class class="StandardProfileAspect" />
        </Target>
    </ECRelationshipClass>
    <ECEntityClass typeName="RectangleProfile">
        <BaseClass>CenteredProfile</BaseClass>
        <BaseClass>ICenterLineProfile</BaseClass>
    </ECEntityClass>
    <ECEntityClass typeName="RoundedRectangleProfile">
        <BaseClass>CenteredProfile</BaseClass>
        <BaseClass>ICenterLineProfile</BaseClass>
        <ECProperty propertyName="RoundingRadius" typeName="double"/>
    </ECEntityClass>
    <ECEntityClass typeName="TransformedProfile" description="A DerivedProfile with only mirroring, offsetting, and rotation.">
        <BaseClass>DerivedProfile</BaseClass>
        <ECProperty propertyName="MirrorShapeAboutYAxis" typeName="boolean"/>
        <ECProperty propertyName="Offset" typeName="point2d"/>
        <ECProperty propertyName="Rotation" typeName="double"/>
        <!--not sure of the TYPE of this 'CardinalPoint' property, placeholder for now-->
        <ECProperty propertyName="CardinalPoint" typeName="int"/>
    </ECEntityClass>
    <ECEntityClass typeName="TrapeziumProfile">
        <BaseClass>CenteredProfile</BaseClass>
        <ECProperty propertyName="TopXOffset" typeName="double"/>
        <ECProperty propertyName="YDim" typeName="double"/>
        <ECProperty propertyName="BottomXDim" typeName="double"/>
        <ECProperty propertyName="TopXDim" typeName="double"/>
    </ECEntityClass>
    <ECEntityClass typeName="TShapeProfile" description="A T-shaped Profile similar to rolled steel T-shapes.">
        <BaseClass>CenteredProfile</BaseClass>
        <ECProperty propertyName="Depth" typeName="double"/>
        <ECProperty propertyName="FlangeWidth" typeName="double"/>
        <ECProperty propertyName="WebThickness" typeName="double"/>
        <ECProperty propertyName="FlangeThickness" typeName="double"/>
        <!--Optional Properties-->
        <ECProperty propertyName="FilletRadius" typeName="double"/>
        <ECProperty propertyName="FlangeSlope" typeName="double"/>
        <ECProperty propertyName="FlangeEdgeRadius" typeName="double"/>
        <ECProperty propertyName="WebSlope" typeName="double"/>
        <ECProperty propertyName="WebEdgeRadius" typeName="double"/>
    </ECEntityClass>
    <ECEntityClass typeName="ZShapeProfile" description="A Z-shaped Profile similar to rolled steel Z-shapes.">
        <BaseClass>CenteredProfile</BaseClass>
        <ECProperty propertyName="Depth" typeName="double"/>
        <ECProperty propertyName="FlangeThickness" typeName="double"/>
        <ECProperty propertyName="FlangeWidth" typeName="double"/>
        <ECProperty propertyName="WebThickness" typeName="double"/>
        <!--Optional Properties-->
        <ECProperty propertyName="EdgeRadius" typeName="double"/>
        <ECProperty propertyName="FilletRadius" typeName="double"/>
    </ECEntityClass>
    <ECEntityClass typeName="TTProfile">
        <!--TODO: Add class mixins/properties etc. as necessary-->
        <BaseClass>CenteredProfile</BaseClass>
    </ECEntityClass>
    <ECEntityClass typeName="RegularPolygonProfile">
        <!--TODO: Add class mixins/properties etc. as necessary-->
        <BaseClass>CenteredProfile</BaseClass>
    </ECEntityClass>
    <ECEntityClass typeName="CapsuleProfile">
        <!--TODO: Add class mixins/properties etc. as necessary-->
        <BaseClass>CenteredProfile</BaseClass>
    </ECEntityClass>
    <ECEntityClass typeName="StandardProfileAspect">
        <BaseClass>bis:ElementUniqueAspect</BaseClass>
        <ECProperty propertyName="Manufacturer" typeName="string" description="The manufacturer of the sections (certain manufacturers may make sections with deviations from the base standard)." />
        <ECProperty propertyName="Revision" typeName="string" description="The revision date or number of the standard" />
        <ECProperty propertyName="StandardsOrganization" typeName="string" description="The organization that creates the table where this section appears" />
    </ECEntityClass>
</ECSchema>
