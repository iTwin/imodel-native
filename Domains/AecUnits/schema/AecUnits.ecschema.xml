<?xml version="1.0" encoding="UTF-8"?>
<ECSchema schemaName="AecUnits" alias="AECU" version="01.00.00" description="This contains the unit definitions that are use across AE Schemas" displayLabel="Design Modeling Units" xmlns="http://www.bentley.com/schemas/Bentley.ECXML.3.1">
    <ECSchemaReference name="Bentley_Standard_CustomAttributes" version="01.00.13" alias="bsca"/>
    <KindOfQuantity typeName="AREA"                         displayLabel="Area"                         persistenceUnit="SQ.M(DefaultReal)"       presentationUnits="SQ.M(real4);SQ.FT(real4u)"                                   relativeError="0.0001"/>
    <KindOfQuantity typeName="AREA_SMALL"                   displayLabel="Small Area"                   persistenceUnit="SQ.M(DefaultReal)"       presentationUnits="SQ.MM(real4);SQ.IN(real4u)"                                  relativeError="0.0001"/>
    <KindOfQuantity typeName="AREA_LARGE"                   displayLabel="Large Area"                   persistenceUnit="SQ.M(DefaultReal)"       presentationUnits="SQ.KM(real4);SQ.MILE(real4u)"                                relativeError="0.01"/>
    <KindOfQuantity typeName="LENGTH"                       displayLabel="Length"                       persistenceUnit="M(DefaultReal)"          presentationUnits="M(real4);MM(real4);FT(fi8);FT(fi16);FT(real4u)"              relativeError="0.00001"/>
    <KindOfQuantity typeName="LENGTH_LONG"                  displayLabel="Long Length"                  persistenceUnit="M(DefaultReal)"          presentationUnits="KM(real4);M(real4);FT(fi8);FT(fi16);FT(real4u)"              relativeError="0.0001"/>
    <KindOfQuantity typeName="LENGTH_SHORT"                 displayLabel="Short Length"                 persistenceUnit="M(DefaultReal)"          presentationUnits="MM(real4);CM(real4);FT(fi8);IN(fi8);IN(fi16);IN(real4u)"     relativeError="0.01"/>
    <KindOfQuantity typeName="VOLUME"                       displayLabel="Volume"                       persistenceUnit="CUB.M(DefaultReal)"      presentationUnits="CUB.M(real4);CUB.FT(real4u);CUB.YRD(real4u)"                 relativeError="0.0001"/>
    <KindOfQuantity typeName="VOLUME_SMALL"                 displayLabel="Small Volume"                 persistenceUnit="CUB.M(DefaultReal)"      presentationUnits="CUB.MM(real4);CUB.CM(real4);CUB.IN(real4u);CUB.FT(real4u)"   relativeError="0.0001"/>
    <KindOfQuantity typeName="VOLUME_LARGE"                 displayLabel="Large Volume"                 persistenceUnit="CUB.M(DefaultReal)"      presentationUnits="CUB.KM(real4);CUB.M(real4);CUB.YRD(real4u);CUB.FT(real4u)"   relativeError="0.01"/>
    <KindOfQuantity typeName="LIQUID_VOLUME"                displayLabel="Liquid Volume"                persistenceUnit="CUB.M(DefaultReal)"      presentationUnits="LITRE(real4);GALLON(real4)"                                  relativeError="0.0001"/>
    <KindOfQuantity typeName="LIQUID_VOLUME_SMALL"          displayLabel="Liquid Small Volume"          persistenceUnit="CUB.M(DefaultReal)"      presentationUnits="LITRE(real4);GALLON(real4)"                                  relativeError="0.0001"/>
    <KindOfQuantity typeName="LIQUID_VOLUME_LARGE"          displayLabel="Liquid Large Volume"          persistenceUnit="CUB.M(DefaultReal)"      presentationUnits="THOUSAND_LITRE(real4);THOUSAND_GALLON(real4)"                relativeError="0.01"/>
    <KindOfQuantity typeName="PROCESS_PIPING_PRESSURE"      displayLabel="Process Piping Pressure"      persistenceUnit="PA(DefaultReal)"         presentationUnits="PA_GAUGE(real4);PA(real4);PSIG(real4);PSI(real4)"            relativeError="0.0001"/>
    <KindOfQuantity typeName="PROCESS_PIPING_TEMPERATURE"   displayLabel="Process Piping Temperature"   persistenceUnit="CELSIUS(DefaultReal)"    presentationUnits="CELSIUS(real4);FAHRENHEIT(real4)"                            relativeError="0.01"/>
    <KindOfQuantity typeName="PROCESS_PIPING_FLOW"          displayLabel="Process Piping Flow Rate"     persistenceUnit="CUB.M/SEC(DefaultReal)"  presentationUnits="LITRE/MIN(real4);CUB.FT/MIN(real4);GALLON/MIN(real4)"        relativeError="0.00001"/>
    <KindOfQuantity typeName="WEIGHT"                       displayLabel="Weight"                       persistenceUnit="KG(DefaultReal)"         presentationUnits="KG(real4);LBM(real4u)"                                       relativeError="0.001"/>
    <KindOfQuantity typeName="CURRENT"                      displayLabel="Current"                      persistenceUnit="A(DefaultReal)"          presentationUnits="A(real4);KILOAMPERE(real4u)"                                 relativeError="0.001"/>
</ECSchema>
