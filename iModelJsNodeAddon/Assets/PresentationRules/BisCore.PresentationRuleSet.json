            ]
          },
          "requiredDirection": "Forward",
          "isPolymorphic": true
        }
      ]
    },
    {
      "ruleType": "ContentModifier",
      "class": {
        "schemaName": "BisCore",
        "className": "GraphicalElement3d"
      },
      "relatedProperties": [
        {
          "relationships": {
            "schemaName": "BisCore",
            "classNames": [
              "GraphicalElement3dRepresentsElement"
            ]
          },
          "relatedClasses": {
            "schemaName": "BisCore",
            "classNames": [
              "Element"
            ]
          },
          "requiredDirection": "Forward",
          "isPolymorphic": true
        }
