// See https://aka.ms/vscode-remote/devcontainer.json for format details.
{
	"name": "imodel02-linux",
	"dockerFile": "./Dockerfile",
	"context": ".",
	"runArgs": [
		"--cap-add=SYS_PTRACE",
		"--security-opt", "seccomp=unconfined",
		"--mount", "src=${env:SrcRoot},dst=/workspace/src,type=bind,consistency=cached"
	],
	"workspaceMount": "",
	"workspaceFolder": "/workspace/src/imodel02",
	"extensions": [
		"ms-vscode.cpptools"
	]
}