        <SubProduct ProductName="iModelBridgeAssign" PartFile="iModelBridgeCore/DgnDbSync/iModelBridge" Repository="imodel02"/>
        <SubPart PartName="RunDgnV8ConverterTests" PartFile="iModelBridgeCore/DgnDbSync/DgnDbSync" Repository="imodel02"/>
    </Part>

    <Part Name="iModelBridgeCore.Dev" PrgOutputDir="iModelBridgeCore">
        <SubPart PartName="iModelCorePRG" PartFile="iModelCore/iModelCore" Repository="imodel02"/>
        <SubPart PartName="iModelBridgeCore"/>
    </Part>
</BuildContext>
