BUILD_NO_STACK_CHECK        = 1

%include mdl.mki

#----------------------------------------------------------
# Set and then include the CompileOptionsMki file.
# It is needed and also %included by PreCompileHeader.mki
#----------------------------------------------------------
CompileOptionsMki               = $(baseDir)imagepp.mki
%include $(CompileOptionsMki)

# Unfortunately, we cannot do this third party stuff in imagepp.mki because TARGET_PLATFORM is not defined yet
%if $(TARGET_PLATFORM) == "Windows"
    %if defined (__IPP_EXTERNAL_THIRD_PARTY_SUPPORTED)
        # PDF library dependency    
        pdflsdkInc=$(BuildContext)VendorAPI/adobe/
        pdflsdkLib=$(BuildContext)SubParts/Libs/
    
        # Oracle library dependency
        oracleInc=$(BuildContext)VendorAPI/oracle/
        oracleLib=$(BuildContext)SubParts/Libs/

        %if !defined(DEBUG_RUNTIME) 

            # Have to include oracle dir because it has nested includes that don't use quotes.
            dirToSearch = $(oracleInc)
            %include cincapnd.mki

            #To satisfy PDFLInitHFT.c and PDFLInitCommon.c included by all/gra/hrf/src/HRFPDFLibInterface.cpp
            dirToSearch=$(pdflsdkInc)Headers/
            %include cincapnd.mki
        %endif
    
        #to statisfy pdflsdk\Include\Headers\environ.h
        nameToDefine=WIN_ENV
        %include cdefapnd.mki
    
        #to statisfy pdf
        nameToDefine=WIN_PLATFORM
        %include cdefapnd.mki

        #to statisfy pdf
        nameToDefine=PRODUCT=\"HFTLibrary.h\"
        %include cdefapnd.mki
    
        nameToDefine=STRICT_HFT_VERSION_CHECKING
        %include cdefapnd.mki
    
        nameToDefine = DLL_NAME_FOR_OCI=L\"$(OCIName).dll\"
        %include cdefapnd.mki
    %endif
%endif

# Need to this here because $(OutBuildDir) is not yet defined in imagepp.mki.
dirToSearch                     = $(ippOutPrivateApi)
%include cincapnd.mki

#----------------------------------------------------------------------
#       Create output directories
#----------------------------------------------------------------------
always:
    !~@mkdir $(o)

