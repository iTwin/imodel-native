# Compile the above uniform set of dependency blocks in a single invocation of the Visual C compiler.
# After the below include of MultiCppCompileGo.mki $(MultiCompileObjectList) will represent the
# list of uniform object files created. You may present $(MultiCompileObjectList) to the linker.
#
%include MultiCppCompileGo.mki
objs =% $(MultiCompileObjectList)

#------------------------------------------------------------------------------------------------------------------
# -------------------------- Non Uniform Objects  -----------------------------------------------------------------
#------------------------------------------------------------------------------------------------------------------

%if $(TARGET_PLATFORM) == "Windows"
# ErMapper headers have errors when compiled with 'Zc:strictStrings' disable this option only for these files.
CCompOpts + -Zc:strictStrings-
nonUniformObjs = \
    $(o)HRFErMapperSupportedFile$(oext) \
    $(o)HRFErMapperSupportedFileEditor$(oext)
    
$(o)HRFErMapperSupportedFile$(oext) : $(ippGraDir)hrf/src/HRFErMapperSupportedFile.cpp $(ippAllInc)HRFErMapperSupportedFile.h

$(o)HRFErMapperSupportedFileEditor$(oext) : $(ippGraDir)hrf/src/HRFErMapperSupportedFileEditor.cpp $(ippAllInc)HRFErMapperSupportedFileEditor.h $(ippAllInc)HRFErMapperSupportedFile.h
%endif

#----------------------------------------------------------------------
#       Libraries used
#----------------------------------------------------------------------
BeLibJpegTurbo                  = $(ContextSubPartsStaticLibs)$(libprefix)BeLibJpegTurbo$(stlibext) $(ContextSubpartsLibs)$(libprefix)BeJpeg$(stlibext)
IppJpegLib12Bits                = $(ContextSubpartsLibs)$(libprefix)IppJpeg12Bits$(stlibext)
IppGdalLib                      = $(ContextSubpartsLibs)$(libprefix)IppGdalLib$(stlibext)
PdfLibInitializer               = $(libprefix)PdfLibInitializer$(libext)
PdfLibInitializerExportLib      = $(ContextSubpartsLibs)$(PdfLibInitializer)
BaseGeocoordLib                 = $(ContextSubpartsLibs)$(libprefix)basegeocoord$(libext)
OCIName                         = Oci


#----------------------------------------------------------------------
#       DLL/LIB creation
#----------------------------------------------------------------------
DLM_OBJECT_FILES            = $(objs) $(nonUniformObjs)
DLM_OBJECT_PCH              = $(o)ImageppInternal$(oext)
DLM_DEST                    = $(o)
DLM_SYMB_DEST               = $(o)
DLM_EXPORT_DEST             = $(o)
DLM_OBJECT_DEST             = $(o)
DLM_NO_INITIALIZE_FUNCTION  = 1
DLM_NOENTRY                 = 1

# Note: Even though we know that some external lib are always linked statically, we still put them into the LINKER_LIBRARIES list below.
# That is so that it can be *excluded* from the (static) link of this library when it is built for platforms such as Android and iOS
# that always build static libraries. On such platforms, it is up to the final consumer of these (static) libraries to know and link them all.
# We must *not* try to nest one static library inside another, as that is not supported on all platforms.

LINKER_LIBRARIES =  $(BeLibJpegTurbo) \
                    $(IppJpegLib12Bits) \
                    $(ContextSubPartsStaticLibs)$(libprefix)BePng$(stlibext) \
                    $(ContextSubPartsStaticLibs)$(libprefix)BeZLib$(stlibext) \
                    $(ContextSubpartsLibs)$(libprefix)BeSQLite$(libext) \
                    $(ContextSubpartsLibs)$(libprefix)MatrixFromTiePts$(stlibext) \
                    $(ContextSubpartsLibs)$(libprefix)BeXml$(libext) \
                    $(ContextSubpartsLibs)$(libprefix)BentleyGeom$(libext) \
                    $(BaseGeocoordLib) \
                    $(ContextSubpartsLibs)$(libprefix)BeCurl$(libext)

%if $(TARGET_PLATFORM)=="Windows"
        # For HRFWebFile.cpp
        LINKER_LIBRARIES + Urlmon.lib

        # GDAL 
        LINKER_LIBRARIES + $(IppGdalLib) wsock32.lib ws2_32.lib

        # ECW/Jpeg2000 support (delay-load removed)
        # Delaying the load of NCSEcw.lib creates a linker error due to the import of the data symbol 
        # "__declspec(dllimport) const NCS::CMutex::`vftable". 
        LINKER_LIBRARIES + $(ContextSubpartsLibs)NCSEcw.lib 

        %if defined (__IPP_EXTERNAL_THIRD_PARTY_SUPPORTED)
            # Adobe pdf support 
            LINKER_LIBRARIES_DELAYLOADED   + $(pdflsdkLib)AdobePDFL.lib  $(PdfLibInitializerExportLib) 
    
            # Oracle support 
            LINKER_LIBRARIES_DELAYLOADED   + $(oracleLib)$(OCIName).lib 
        %endif
    
        # MrSID support
        LINKER_LIBRARIES_DELAYLOADED   + $(ContextSubpartsLibs)lti_dsdk.lib $(ContextSubpartsLibs)lti_dsdk_cdll.lib 
%endif

%include $(sharedMki)linkLibrary.mki

