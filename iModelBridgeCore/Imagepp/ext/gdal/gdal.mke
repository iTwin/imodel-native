cDefs + -DFRMT_ceos -DFRMT_bsb -DFRMT_aigrid -DFRMT_elas -DFRMT_hfa -DFRMT_gtiff -DFRMT_sdts -DFRMT_raw -DFRMT_gxf -DFRMT_ceos2 \
