                      -m"$(OutBuildContexts)ImagePP/VendorAPI/ImagePP-GdalLib=$(ippGdalDir)port/cpl_config.h.an" \
                      -m"$(OutBuildContexts)ImagePP/VendorAPI/ImagePP-GdalLib=$(ippGdalDir)ogr/*.h"

