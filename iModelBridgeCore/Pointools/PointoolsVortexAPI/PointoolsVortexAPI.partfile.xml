<!--
********************************************************************************************************
* PointoolsVortexAPI Default Compiler Parts (does not build PointoolsIO)
********************************************************************************************************
-->
    <!-- Bind my own PublicAPI into BuildContext/PublicAPI. Note that this part does not cause any sub-part PublicAPIs to be bound. Also deliver my license notice. -->
    <Part Name="PublicApi" BentleyBuildMakeFile="PointoolsVortexAPI.prewire.mke">
        <Bindings>
            <PublicAPI Domain="Vortex" />
        </Bindings>
    </Part>

    <!-- On iOS platforms, libraries must be static. That's why we make a distinction here between non-iOS and iOS platforms -->
    <Part Name="VortexLibs">
        <SubPart PartName="VortexLibsNonIOS"/>
        <SubPart PartName="VortexLibsIOS"/>
    </Part>
    <Part Name="VortexLibsNonIOS" ExcludePlatforms="iOS*">
        <SubPart PartName="PointoolsVortexAPI_DLL"/>
        <SubPart PartName="PointoolsVortexAPI_FeatureExtract_DLL"/>
    </Part>
    <Part Name="VortexLibsIOS" OnlyPlatforms="iOS*">
        <SubPart PartName="PointoolsVortexAPI_DLL" LibType="Static"/>
        <SubPart PartName="PointoolsVortexAPI_FeatureExtract_DLL"/>
    </Part>
    
    <!-- HAVE_OPENGL: disabled for now -->
    <Part Name="OpenGLSubParts" OnlyPlatforms="x86,x64" PrgOutputDir="PointoolsVortexAPI" >
        <SubPart PartName="EncryptShaders" />        
        <SubPart PartName="OpenGLExt"       PartFile="openglHeaders"    Repository="libsrc-opengl"/>
        <SubPart PartName="GLEW"            PartFile="opengl"           Repository="libsrc-opengl" LibType="Static"/>
    </Part>

    <Part Name="PointoolsVortexAPI_DLL" BentleyBuildMakeFile="PointoolsVortexAPI.mke" ApiNumber="G06">
        <RequiredRepository>PointoolsCommon</RequiredRepository>
        <SubPart PartName="PublicApi" />
        <SubPart PartName="BentleyDll"      PartFile="Bentley"           Repository="Bentley" />

        <!-- <SubPart PartName="LicenseSDK8.11" PartFile="${SrcRoot}bsicommon/CvsPartfiles/bsibin" Repository="bsibin-licensesdk" />  -->
        <!--SubPart PartName="OpenGLSubParts"/-->
        <SubPart PartName="ttl"             PartFile="ttl"              Repository="libsrc-ttl"/>
        <SubPart PartName="Loki"            PartFile="Loki"             Repository="libsrc-loki"/>
        <SubPart PartName="FastDelegate"    PartFile="FastDelegate"     Repository="libsrc-FastDelegate"/>
        <SubPart PartName="wildmagic"       PartFile="wildmagic"        Repository="libsrc-geometrictools"/>        
        <SubPart PartName="SCZ_Compress"    PartFile="scz_compress"     Repository="libsrc-scz_compress"/>
        <Bindings>
            <Libs>Delivery/$(shlibprefix)PointoolsVortexAPI$(libext)</Libs>
            <Assemblies ProductDirectoryName="PointoolsAssemblies">Delivery/$(shlibprefix)PointoolsVortexAPI$(ApiNumber)$(shlibext)</Assemblies>
        </Bindings>
    </Part>

    <Part Name="EncryptShaders" BentleyBuildMakeFile="EncryptShaders/EncryptShaders.mke" OnlyPlatforms="x86,x64">
        <RequiredRepository>PointoolsCommon</RequiredRepository>
        <Bindings>
            <Files> Delivery/enc-shaders.exe </Files>
        </Bindings>
    </Part>
  
<!--
********************************************************************************************************
* Pointools Vortex FeatureExtract
********************************************************************************************************
-->
  <Part Name="PointoolsVortexAPI_FeatureExtract_DLL" OnlyPlatforms="x86,x64" BentleyBuildMakeFile="extensions/VortexFeatureExtract/FeatureExtract.mke" >
    <RequiredRepository>PointoolsCommon</RequiredRepository>	
    <SubPart PartName="wildmagic" PartFile="wildmagic" Repository="libsrc-geometrictools"/>
    <SubPart PartName="PointoolsVortexAPI_DLL"/>
    <Bindings>
      <VendorAPI Domain="PointoolsVortexAPI_FeatureExtract_DLL"/>
      <Libs> Delivery/VortexFeatureExtract.lib </Libs>
      <Assemblies ProductDirectoryName="PointoolsAssemblies"> Delivery/VortexFeatureExtract.dll </Assemblies>
    </Bindings>
  </Part>
  

