<BuildContext xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="..\..\bsicommon\build\PartFile.xsd">

    
  <Part Name="PointoolsVortexAPI_VS2005" PrgOutputDir="PointoolsVortexAPI">

    <Directories DirectoryListName="PointoolsVortexAPIDirectoryList_VS2005_NULL"/>              <!-- Do not deliver product in this part -->
    
    <SubPart PartName="PointoolsVortexAPI_DLL_VS2005"/>
    
    <SubPart PartName="PointoolsVortexAPI_FeatureExtract_DLL_VS2005" />
    
    <SubPart PartName="PointoolsIO_VS2005" PartFile="${srcroot}Pointools\PointoolsIO\PointoolsIO_VS2005" Repository="PointoolsIO" />

  </Part>
  

<!--
********************************************************************************************************
* PointoolsVortexAPI VS2005 Part
********************************************************************************************************
-->
  
  <Part Name="EncryptShaders_VS2005" BMakeFile="EncryptShaders\EncryptShaders.mke" BMakeOptions="+dPOINTOOLS_BUILD_USING_VS2005=1">

    <SubPart PartName="PointoolsCommon" PartFile="${srcroot}Pointools\PointoolsCommon\PointoolsCommon" Repository="PointoolsCommon"/>

    <Bindings>
      <Assemblies ProductDirectoryName="PointoolsAssemblies">
        Delivery\enc-shaders.exe
      </Assemblies>
    </Bindings>
  </Part>

  
  <Part Name="PointoolsVortexAPI_Vortex_DLL_Makefile_VS2005" BMakeFile="Makefile\PointoolsVortexAPI_Vortex_DLL_Makefile.mke" BMakeOptions="+dPOINTOOLS_BUILD_USING_VS2005=1">

    <SubPart PartName="PointoolsCommon" PartFile="${srcroot}Pointools\PointoolsCommon\PointoolsCommon" Repository="PointoolsCommon"/>
 
  </Part>

  
  <Part Name="PointoolsVortexAPI_DLL_VS2005" BMakeFile="PointoolsVortexAPI_DLL.mke" BMakeOptions="+dPOINTOOLS_BUILD_USING_VS2005=1">

    <SubPart PartName="PointoolsCommon" PartFile="${srcroot}Pointools\PointoolsCommon\PointoolsCommon" Repository="PointoolsCommon"/>

    <SubPart PartName="PointoolsVortexAPI_Vortex_DLL_Makefile_VS2005" BMakeOptions="+dPOINTOOLS_BUILD_USING_VS2005=1"/>

    <SubPart PartName="EncryptShaders_VS2005"/>

    <SubPart PartName="PointoolsLibsVortex_2_VS2005" PartFile="${srcroot}Pointools\PointoolsCommon\PointoolsLibs\PointoolsLibs_VS2005" Repository="PointoolsLibs"/>

    <Bindings>
      <VendorAPI Domain="PointoolsVortexAPI_DLL"/>
      <Libs>
        Delivery\PointoolsVortexAPI.lib
      </Libs>
      <Assemblies ProductDirectoryName="PointoolsAssemblies">
        Delivery\PointoolsVortexAPI.dll
      </Assemblies>
    </Bindings>
  </Part>


<!--
********************************************************************************************************
* Pointools Vortex FeatureExtract VS2005 Part
********************************************************************************************************
-->

  <Part Name="PointoolsVortexAPI_FeatureExtract_DLL_Makefile_VS2005" BMakeFile="Makefile\PointoolsVortexAPI_FeatureExtract_DLL_Makefile.mke" BMakeOptions="+dPOINTOOLS_BUILD_USING_VS2005=1">

    <SubPart PartName="PointoolsCommon" PartFile="${srcroot}Pointools\PointoolsCommon\PointoolsCommon" Repository="PointoolsCommon"/>
    
  </Part>

  <Part Name="PointoolsVortexAPI_FeatureExtract_DLL_VS2005" BMakeFile="PointoolsVortexAPI_FeatureExtract_DLL.mke" BMakeOptions="+dPOINTOOLS_BUILD_USING_VS2005=1">

    <SubPart PartName="PointoolsCommon" PartFile="${srcroot}Pointools\PointoolsCommon\PointoolsCommon" Repository="PointoolsCommon"/>

    <SubPart PartName="PointoolsVortexAPI_FeatureExtract_DLL_Makefile_VS2005"/>

    <SubPart PartName="WildMagic4_VS2005" PartFile="${srcroot}Pointools\PointoolsCommon\PointoolsLibs\PointoolsLibs_VS2005" Repository="PointoolsLibs"/>

    <Bindings>
      
      <VendorAPI Domain="PointoolsVortexAPI_FeatureExtract_DLL" />

      <Libs>
        Delivery\VortexFeatureExtract.lib
      </Libs>
      
      <Assemblies ProductDirectoryName="PointoolsAssemblies">
        Delivery\VortexFeatureExtract.dll
      </Assemblies>

    </Bindings>

  </Part>


</BuildContext>
