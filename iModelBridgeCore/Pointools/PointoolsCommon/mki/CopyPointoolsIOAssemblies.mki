    ~linkfile "$(PointoolsIOAssemblyDest)\IQOpen.dll=$(PointoolsIOAssemblySource)IQOpen.dll"
    ~linkfile "$(PointoolsIOAssemblyDest)\jsoncpp.dll=$(PointoolsIOAssemblySource)jsoncpp.dll"
    ~linkfile "$(PointoolsIOAssemblyDest)\laszip.dll=$(PointoolsIOAssemblySource)laszip.dll"
    ~linkfile "$(PointoolsIOAssemblyDest)\libxml2.dll=$(PointoolsIOAssemblySource)libxml2.dll"
