%message *********************************************************************
%message * Copying PointoolsIO Third Party DLLs
%message *
%message * Source                : $(PointoolsIOAssemblySource)
%message * Dest                  : $(PointoolsIOAssemblyDest)
%message * Target Architecture   : $(TARGET_PROCESSOR_ARCHITECTURE)
%if defined(POINTOOLS_BUILD_USING_VS2005)
%message * Visual Studio Version : VS2005
%endif
%if defined(POINTOOLS_BUILD_USING_VS2012)
%message * Visual Studio Version : VS2012
%endif
%message *
%message *********************************************************************


%if $(TARGET_PROCESSOR_ARCHITECTURE) == "x86"

%if defined(POINTOOLS_BUILD_USING_VS2005)

