CCPchOpts = $(UsePrecompiledHeaderOptions) -DSCALABLE_TERRAIN_MODEL_PRIVATE_SECTION -DVANCOUVER_API

%if !defined (3DMesh)
CCPchOpts + -DNO_3D_MESH
%endif

