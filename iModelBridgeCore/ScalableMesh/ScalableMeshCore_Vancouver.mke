baseDir         = $(_MakeFilePath)
PolicyFile      = $(SrcRoot)ScalableMesh/mki/AssertScalableMeshCorePolicy.mki
SolutionPolicyMki=$(baseDir)ScalableMeshCore.mki
#BUILD_USING_VS2013=1
OPTIMIZE_FOR_SPEED=1

%include  mdl.mki
appName     = ScalableMeshCore
DLM_NAME    = $(appName)
CCompPDBName = $(appName)


#----------------------------------------------------------------------
#       Create output directories
#----------------------------------------------------------------------
always:
    !~@mkdir $(o)

#----------------------------------------------------------------------
#   Symlink APIs
#----------------------------------------------------------------------
always:
   @CreateSymLinks.py -d"$(BuildContext)PublicAPI/ScalableMesh=$(SCMPublicAPISrc)/ScalableMesh"


always:
   @CreateSymLinks.py -f"$(BuildContext)VendorAPI/TilePublisher/TilePublisher.h=$(tilePublisherDir)TilePublisher.h"
   @CreateSymLinks.py -f"$(BuildContext)VendorAPI/TilePublisher/Constants.h=$(tilePublisherDir)Constants.h"
   @CreateSymLinks.py -f"$(BuildContext)VendorAPI/TilePublisher/MeshTile.h=$(tilePublisherDir)MeshTile.h"


#CCompOpts + -openmp -bigobj -DCGAL_NO_AUTOLINK -DBOOST_ALL_NO_LIB -DBOOST_SYSTEM_NO_DEPRECATED -wd4091 -wd4312


#Uncomment this to allow generating design meshes
#CCompOpts + -DWIP_MESH_IMPORT 

#For legal reasons, not compiling these libraries or code depending on them right now.
%if !defined (3DMesh)
CCompOpts + -DNO_3D_MESH
%endif

%if !defined (GarlandFilter)
CCompOpts + -DNO_GARLAND_FILTER 
%endif
#----------------------------------------------------------------------
#       Inform user of compile options
#----------------------------------------------------------------------
always:
        |  Compiler options: $(cDefs)$(cDefsPost) $(copt) $(cIncs) $(PublicApiIncludes) $(CCompOpts)
        |  -------- --------

CCompOpts + $(PublicApiIncludes) -DCORE_LIB_TEST 

#disable warning: "'argument': conversion from 'int64_t' to 'uint32_t', possible loss of data". NEEDS_WORK_SM: switch all node ID types to uint64_t once and for all, then change this.
CCompOpts + -wd4244

#release builds: error in boost (named_function_params.hpp)
#CCompOpts + -wd4172

#x86 build. signed/unsigned mismatch and int64/size_t issues.
CCompOpts + -wd4018 -wd4309 -wd4305
