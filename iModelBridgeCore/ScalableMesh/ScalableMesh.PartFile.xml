            <Assemblies ProductDirectoryName="MdlsysAsneeded">delivery/$(shlibprefix)ScalableMesh$(shlibext)</Assemblies> 
            <PublicAPI Domain="ScalableMesh" />
        </Bindings>
    </Part>
    
