LicensingMkiPublicApiDir = $(ClientPublicApiDirectory)Licensing/
LicensingMkiSourceDir = $(ClientSourceDirectory)Licensing/

#--------------------------------------------------------------------------------------
#   Set up build dependencies
#--------------------------------------------------------------------------------------
LicensingMkiHeaders =   $(LicensingMkiPublicApiDir)MobileTracking.h \
                        $(LicensingMkiPublicApiDir)UsageTracking.h                     

#--------------------------------------------------------------------------------------
#   Compile
#--------------------------------------------------------------------------------------
$(o)MobileTracking$(oext)       : $(LicensingMkiSourceDir)MobileTracking.cpp $(LicensingMkiHeaders) ${MultiCompileDepends}

$(o)UsageTracking$(oext)        : $(LicensingMkiSourceDir)UsageTracking.cpp $(LicensingMkiHeaders) ${MultiCompileDepends}
