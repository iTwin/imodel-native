        </BootstrapperApplicationRef>

        <WixVariable Id="WixMbaPrereqPackageId" Value="Nothing" />
        <WixVariable Id="WixMbaPrereqLicenseUrl" Value="Nothing.rtf" />
        
        <Chain ParallelCache="yes">
          <PackageGroupRef Id="PreRequisites"/>
