<?xml version="1.0" encoding="utf-8"?>

<BuildContext xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../bsicommon/build/PartFile.xsd">

    <Part Name="DgnDb0601Assemblies" BentleyBuildMakeFile="BimTeleporter/DgnDb0601AssembliesPrewire.mke">
        <NuGetPackage targetFramework="native">DgnDb_1_6_ForUpgraderNuget</NuGetPackage>
        <Bindings>
            <Assemblies ProductDirectoryName="DgnDbAssemblies" Required="false">
                Delivery/*.dll,
            </Assemblies>
            <Directory SourceName="Delivery/DgnDbLibs" ProductDirectoryName="DgnDb06Libs" SubPartDirectory="DgnDbLibs" Required="false"/>
            <Directory SourceName="Delivery/DgnDbPlatformAssets" ProductDirectoryName="DgnDb06PlatformAssets" SubPartDirectory="DgnDbPlatformAssets" Required="false"/>
            <VendorAPI Domain="DgnDb06Api"/>
        </Bindings>
    </Part>

    <Part Name="BisJson1Exporter0601" BentleyBuildMakeFile="BimTeleporter/BimExporter0601/lib/BisJson1Exporter0601.mke" OnlyPlatforms="x64,WinRTX64" ExcludeLibType="Static">
        <SubPart PartName="DgnDb0601Assemblies"/>
        <SubPart PartFile="DgnDbSync" PartName="TeleporterPublicAPI"/>
        <Bindings>
            <Libs ProductDirectoryName="BimTeleporterLibs" Required="false">Delivery/$(libprefix)BisJson1Exporter0601$(libext)</Libs>
            <Assemblies>Delivery/$(shlibprefix)BisJson1Exporter0601$(shlibext)</Assemblies>
        </Bindings>
    </Part>

    <Part Name="BimExporter0601Exe" BentleyBuildMakeFile="BimTeleporter/BimExporter0601/exe/BimExporter0601.mke" OnlyPlatforms="x64" ExcludeLibType="Static">
        <SubPart PartName="BisJson1Exporter0601"/>
        <Bindings>
            <Files ProductDirectoryName="ApplicationRoot">Delivery\BimExporter0601.exe</Files>
            <Files ProductDirectoryName="DgnPlatformXliffs" Required="false" SubPartDirectory="xliffs">Delivery/xliffs/BimExporter0601_en.xliff</Files>
            <Files ProductDirectoryName="PublisherLoggingConfigXml">Delivery/BimTeleporter.logging.config.xml</Files>
        </Bindings>
    </Part>

    <Product Name="BimExporter0601">
        <SubPart PartName="BimExporter0601Exe"/>
        <Directories DirectoryListName="DgnDbSyncAppCommonDirList" PartFile="DgnDbSync" Repository="DgnDbSync"/>
        <Directories DirectoryListName="BimTeleporterDirList"  PartFile="DgnDbSync" Repository="DgnDbSync"/>
    </Product>

</BuildContext>

