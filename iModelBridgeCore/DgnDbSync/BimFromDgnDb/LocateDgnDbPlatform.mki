DGNDB_VER=DgnDb06

# -----------------------------------------------------------------------------------------------------------------------
# Allow the developer to work from a live build of Dgndb, instead of LKGs
# -----------------------------------------------------------------------------------------------------------------------
%ifdef DGNDB0601_DGNPLATFORM_NUGET
    DgnDbDgnPlatformBase = $(DGNDB0601_DGNPLATFORM_NUGET)
    %ifnofile ${DgnDbDgnPlatformBase}
        %error $(DgnDbDgnPlatformBase) not found. Make sure DGNDB0601_DGNPLATFORM_NUGET points to Product\DgnDb_1_6_ForUpgraderNuget\native\ in your DgnDb output tree.
    %endif
%else
    DgnDbDgnPlatformBase = $(BuildContext)nuget/native/DgnDb_1_6_ForUpgraderNuget/native/
%endif

DgndbDgnPlatformLibDir = $(DgnDbDgnPlatformBase)lib/
DgndbDgnPlatformIncludeDir = $(DgnDbDgnPlatformBase)include/
