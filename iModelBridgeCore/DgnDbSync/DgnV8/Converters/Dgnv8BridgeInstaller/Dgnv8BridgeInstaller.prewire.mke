$(BuildContext)DgnV8BridgeInstallFiles :
    !~@mkdir $@

$(BuildContext)DgnV8BridgeInstallFiles/DgnV8BridgeInstallerBeforeCompile.mki : $(_MakeFilePath)DgnV8BridgeInstallerBeforeCompile.mki
    @CreateSymLinks.py -f"$@=$<"

$(BuildContext)DgnV8BridgeInstallFiles/DgnV8BridgeInstaller.wxs : $(_MakeFilePath)DgnV8BridgeInstaller.wxs
    @CreateSymLinks.py -f"$@=$<"
