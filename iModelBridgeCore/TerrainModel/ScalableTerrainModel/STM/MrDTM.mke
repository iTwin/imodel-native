   @bcopy -cru $(BC_DESKTOP)$(DLM_NAME).dll $(OutMstn)Bentley\Microstation\mdlsys\asneeded\$(DLM_NAME).dll
   @bcopy -cru $(BC_DESKTOP)$(DLM_NAME).dll $(OutPowerPlatform)Bentley\PseudoStation\mdlsys\asneeded\$(DLM_NAME).dll       
