    -@cacls $(MS)mdlsys\asneeded\$(DLM_NAME).dll \/E \/G Everyone:F
    -@cacls $(MS_EXTRAS)bsilib\$(appName).lib \/E \/G Everyone:F
    @bcopy -cru $(BC_DESKTOP)$(DLM_NAME).dll $(MS)mdlsys\asneeded\$(DLM_NAME).dll 
