<?xml version="1.0" encoding="utf-8"?>
<BuildStrategy
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../bsicommon/build/BuildStrategy.xsd"
    >
    
    <ImportStrategy Name="Base"/>
    
    <DefaultTarget
        Repository="iModel02"
        PartFile="Bridges/C3d/C3dBridge"
        PartName="C3dBridgePRG"
        Platform="x64"
        LKGPlatforms="x64"
        OnlyPlatforms="x64"
        />

    <!-- This is part of the monorepo, so it's expected to build all dependencies. -->
    <ImportStrategy Name="BuildIModelCore"/>
    <ImportStrategy Name="BuildDwgBridge"/>

    <PartStrategy PartFile="C3dBridge" PartName="*" BuildFromSource="Once"/>
    <PartStrategy PartFile="CivilBaseGeometry" PartName="*" BuildFromSource="Once"/>
    <PartStrategy PartFile="CiviliModelBridges" PartName="*" BuildFromSource="Once"/>
    <PartStrategy PartFile="LinearReferencing" PartName="*" BuildFromSource="Once"/>
    <PartStrategy PartFile="RoadRailAlignment" PartName="*" BuildFromSource="Once"/>
    <PartStrategy PartFile="RoadRailPhysical" PartName="*" BuildFromSource="Once"/>
    <PartStrategy PartFile="BridgeStructuralPhysical" PartName="*" BuildFromSource="Once"/>
 
</BuildStrategy>
