<?xml version="1.0" encoding="utf-8"?>
<BuildStrategy
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../bsicommon/build/BuildStrategy.xsd"
    >

    <ImportStrategy Name="Base"/>

    <DefaultTarget
        Repository="iModel02"
        PartFile="TestingScripts/Coverage/Coverage"
        PartName="imodel02-CodeCoverage"
        Platform="x64"
        OnlyPlatforms="x64"
        />

    <!-- Build the various "dependencies" so that test suites are available. -->
    <ImportStrategy Name="BuildIModelCore"/>
    <ImportStrategy Name="BuildIModelBridgeCore"/>
    <ImportStrategy Name="BuildMstnBridge"/>
    <ImportStrategy Name="BuildDwgBridge"/>

    <PartStrategy PartFile="Coverage" PartName="*" BuildFromSource="Once"/>

</BuildStrategy>
