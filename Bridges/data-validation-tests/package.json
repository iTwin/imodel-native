    "test:multi": "node ./node_modules/@bentley/build-tools/scripts/test.js --testDir=\"./lib/test/multi-bridge\"",
    "test:qc": "node ./node_modules/@bentley/build-tools/scripts/test.js --testDir=\"./lib/test/dgnv8converter\"",
    "copy:config": "cpx \"./src/config/**/*\" ./lib/config"
  },
  "dependencies": {
    "@bentley/data-validation-api": "~0"
  },
  "devDependencies": {
    "@bentley/build-tools": "~0.163.0",
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/glob": "^7.1.1",
    "rimraf": "^2.6.2",
    "typescript": "^3.0.0",
    "electron": "^2.0.10",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "glob": "^7.1.1"
  },
